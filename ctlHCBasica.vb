    Private Sub ValidarTextosExamenFisico(ByVal sender As Object, ByVal e As System.ComponentModel.CancelEventArgs)

        If sender.Name = "txtPesoEF" Or sender.Name = "txtTallaEF" Then
            If objDatosPaciente.CodigoUnidadMedidaEdad <> "A" Then
                Exit Sub
            ElseIf objDatosPaciente.CodigoUnidadMedidaEdad = "A" Then
                If objDatosPaciente.Edad < 2 Then
                    Exit Sub
                End If
            End If
            Dim strDescripcionIMC As String
            strDescripcionIMC = ""
            If Val(txtPesoEF.Text) > 0 And Val(txtTallaEF.Text) > 0 Then
                txtValorIMCEF.Text = CalcularIndiceDeMasaCorporal(Val(txtPesoEF.Text), Val(txtTallaEF.Text), strDescripcionIMC)
                txtDescValorIMCEF.Text = strDescripcionIMC
            Else
                txtValorIMCEF.ResetText()
                txtDescValorIMCEF.ResetText()
            End If
        End If
    End Sub

