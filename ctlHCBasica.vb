    Private Sub chlMetodoPlanificacion_ItemCheck(ByVal sender As System.Object, ByVal e As System.Windows.Forms.ItemCheckEventArgs) Handles chlMetodoPlanificacion.ItemCheck
        If e.CurrentValue = CheckState.Checked And chlMetodoPlanificacion.SelectedValue = 10 Then
            lblDefinitivoMP.Visible = False
            txtDefinitivoMP.Visible = False
            txtDefinitivoMP.Obligatorio = False
            txtDefinitivoMP.Text = String.Empty

        End If
        If e.CurrentValue = CheckState.Unchecked And chlMetodoPlanificacion.SelectedValue = 10 Then
            If chlMetodoPlanificacion.GetItemCheckState(6) = 1 Then
                chlMetodoPlanificacion.SetItemChecked(6, False)
            End If
            lblDefinitivoMP.Visible = True
            txtDefinitivoMP.Visible = True
            txtDefinitivoMP.Enabled = True
            txtDefinitivoMP.Obligatorio = True
            txtDefinitivoMP.Text = String.Empty


        End If

        If e.CurrentValue = CheckState.Unchecked And chlMetodoPlanificacion.SelectedValue = 9 Then
            LimpiarMetodoPlanificacion()
        End If

        If e.CurrentValue = CheckState.Unchecked And chlMetodoPlanificacion.SelectedValue <> 9 Then
            If chlMetodoPlanificacion.GetItemCheckState(6) = 1 Then
                chlMetodoPlanificacion.SetItemChecked(6, False)
            End If
        End If

    End Sub

    Private Sub LimpiarMetodoPlanificacion()
        For i As Integer = 0 To chlMetodoPlanificacion.Items.Count - 1
            chlMetodoPlanificacion.SetItemChecked(i, False)
        Next

        chlMetodoPlanificacion.ClearSelected()

        txtDefinitivoMP.Text = ""
        lblDefinitivoMP.Visible = False
        txtDefinitivoMP.Visible = False
        txtDefinitivoMP.Obligatorio = False
    End Sub

