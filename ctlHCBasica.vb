                MessageBox.Show("Error al Actualizar Manejo Esquema Vacunacion")
            End If

            ErrorAntecedenteModulo = ""

        Catch ex As Exception
            ControlAntecedentesHb(ex.Source & " - " & Environment.MachineName.ToString & " - " & ErrorAntecedenteModulo, ex.Message, ex.StackTrace & " - " & ex.TargetSite.ToString)
        End Try

