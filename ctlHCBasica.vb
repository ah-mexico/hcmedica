                    .Item("NoRecuerdaMamografia") = IIf(chbNoRecuerdaMamografia.Checked, 1, 0)
                    .Item("NuncaMamografia") = IIf(chbNuncaMamografia.Checked, 1, 0)
                    .Item("MPDefinitivo") = txtDefinitivoMP.Text.ToString 'ReemplazarString(Val(txtDefinitivoMP.Text.ToString), 0, DBNull.Value)
                    .Item("EmbarazoActual") = Datos(33)
                    .Item("Editable") = 1
                End With
                dtgAntecedentesGinecologicos.DataSource.Rows.Add(dtRow)
                dtgAntecedentesGinecologicos.Height = dtgAntecedentesGinecologicos.ColumnHeadersHeight + (dtgAntecedentesGinecologicos.Rows.GetRowsHeight(DataGridViewElementStates.None))
