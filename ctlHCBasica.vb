    Private Sub dtgAntTraumatologicos_CellContentClick(ByVal sender As System.Object, ByVal e As System.Windows.Forms.DataGridViewCellEventArgs) Handles dtgAntTraumatologicos.CellContentClick

        Try
            If e.RowIndex = -1 Then
                Return
            End If

            If dtgAntTraumatologicos.Rows(e.RowIndex).Cells("EditableAtrauma").Value = 0 Then

                If dtgAntTraumatologicos.Columns(e.ColumnIndex).Name = "ConfirmaTrauma" Then

                    Dim row As DataGridViewRow = dtgAntTraumatologicos.Rows(e.RowIndex)

                    Dim cellSelecion As DataGridViewCheckBoxCell = TryCast(row.Cells("NiegaTrauma"), DataGridViewCheckBoxCell)

                    If Convert.ToBoolean(cellSelecion.Value) Then
                        row.Cells("NiegaTrauma").Value = 0
                    End If

                Else
                    If dtgAntTraumatologicos.Columns(e.ColumnIndex).Name = "NiegaTrauma" Then

                        Dim row As DataGridViewRow = dtgAntTraumatologicos.Rows(e.RowIndex)

                        Dim cellSelecion As DataGridViewCheckBoxCell = TryCast(row.Cells("ConfirmaTrauma"), DataGridViewCheckBoxCell)

                        If Convert.ToBoolean(cellSelecion.Value) Then
                            row.Cells("ConfirmaTrauma").Value = 0
                        End If

                    Else
                        Return
                    End If
                End If

                dtgAntTraumatologicos.CommitEdit(DataGridViewDataErrorContexts.Commit)
                'Else
                'MessageBox.Show("No Es Permitido Modificar Este Registro")
            End If

        Catch ex As Exception

        End Try

    End Sub

    Private Sub dtgAntTraumatologicos_CellValueChanged(ByVal sender As System.Object, ByVal e As System.Windows.Forms.DataGridViewCellEventArgs) Handles dtgAntTraumatologicos.CellValueChanged
        Dim comodin As String
        If e.RowIndex >= 0 Then

            If String.IsNullOrEmpty(dtgAntTraumatologicos.CurrentRow.Cells(e.ColumnIndex).Value.ToString) Then
                comodin = 0
                dtgAntTraumatologicos.Rows(e.RowIndex).Cells(dtgAntTraumatologicos.CurrentRow.Cells(e.ColumnIndex).OwningColumn.Name).Value = 0
            Else
                comodin = dtgAntTraumatologicos.CurrentRow.Cells(e.ColumnIndex).Value.ToString
            End If

            If comodin <> System.DBNull.Value.ToString Then
                ModificarAntecedenteTraumatologico(dtgAntTraumatologicos.CurrentRow, sender, dtgAntTraumatologicos.CurrentRow.Cells(e.ColumnIndex).OwningColumn.HeaderText)
            End If
        End If
    End Sub

    Private Sub dtgAntTraumatologicos_DoubleClick(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles dtgAntTraumatologicos.DoubleClick

        If dtgAntTraumatologicos.DataSource Is Nothing Then
            Exit Sub
        Else
            If dtgAntTraumatologicos.DataSource.Rows.Count <= 0 Then
                Exit Sub
            End If
        End If

        If dtgAntTraumatologicos.CurrentRow.Cells("EditableAtrauma").Value = "1" Then
            EditarAntecedenteTraumatologico(sender.CurrentRow)
        Else
            MessageBox.Show("No Es Permitido Modificar Este Registro")
            Exit Sub
        End If

        strDatoModificado = "ATRAU"

    End Sub
