                    .Item("fec_con") = FechaServidor().ToShortDateString()
                    .Item("NoRefiereMenarquia") = IIf(chbNoRefiereMenarquia.Checked, 1, 0)
                    .Item("NoRecuerdaFUM") = IIf(chbNoRecuerdaFUM.Checked, 1, 0)
