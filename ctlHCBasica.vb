    Private Function ValidarCamposVaciosAPE() As Boolean

        Dim blnRespuesta As Boolean
        blnRespuesta = True

        If txtEdadGestacional.Text.Trim.Length <= 0 Then
            MessageBox.Show("Falta Información en Campo Obligatorio - Edad Gestacional")
            txtEdadGestacional.Focus()
            blnRespuesta = False
            Return blnRespuesta
        End If

        If rbtUnica.Checked = False And rbtMultiple.Checked = False Then
            MessageBox.Show("Falta Información en Campo Obligatorio - Tipo de Gestación")
            blnRespuesta = False
            Return blnRespuesta
        End If

        If chlTipoParto.CheckedItems.Count = 0 Then
            MessageBox.Show("Debe seleccionar por lo menos un Tipo de parto.")
            blnRespuesta = False
            Return blnRespuesta
        End If

        Return blnRespuesta

    End Function

