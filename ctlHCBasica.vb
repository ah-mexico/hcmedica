                'CDQF inclusion retorno y posterior llenado dato analisis y explicacion plan manejo
                If dsDatosID.Tables(0).Rows.Count > 0 Then
                    Valores(10) = dsDatosID.Tables(0).Rows(0)("ExplicaPlanManejo").ToString
                    Valores(11) = dsDatosID.Tables(0).Rows(0)("Analisis").ToString
                Else
                    Valores(10) = ""
                    Valores(11) = ""
                End If

                Valores(1) = dsDatosID.Tables(1).Rows(0)("causaext").ToString

                If Valores(1).ToString.Trim.Length > 0 Then
                    objHistoriaBasica.ImpresionDiagnostica.HabilitarCausaExterna = puedeModificarImpresionDiagn()
                End If

                Valores(2) = dsDatosID.Tables(1).Rows(0)("condusu").ToString
                If Valores(2).ToString.Trim.Length > 0 Then
                    objHistoriaBasica.ImpresionDiagnostica.HabilitarCondicionUsuaria = puedeModificarImpresionDiagn()
                End If

                Valores(9) = dsDatosID.Tables(1).Rows(0)("planManejo").ToString
                If Valores(9).ToString.Length > 0 Then
                    objHistoriaBasica.ImpresionDiagnostica.HabilitarPlanManejo = puedeModificarImpresionDiagn()
                End If

            End If

        End If

        Valores(3) = ""
        Valores(4) = ""
        Valores(5) = 0
        Valores(6) = ""
        Valores(7) = ""
        Valores(8) = ""

        If dsDatosID.Tables.Count > 2 Then
            If dsDatosID.Tables(2).Rows.Count > 0 Then
                Valores(3) = dsDatosID.Tables(2).Rows(0)("SospechaAtep").ToString
                If Valores(3).ToString.Trim.Length > 0 Then
                    If Valores(3).ToString = "S" Then
                        objHistoriaBasica.ImpresionDiagnostica.HabilitarSospechaATEP = puedeModificarImpresionDiagn()
                    End If
                End If
                Valores(4) = dsDatosID.Tables(2).Rows(0)("ampliaOcupa").ToString
                objHistoriaBasica.ImpresionDiagnostica.HabilitarAmpliacionOcupacion = objHistoriaBasica.ImpresionDiagnostica.HabilitarSospechaATEP
                Valores(5) = dsDatosID.Tables(2).Rows(0)("antigueOcupa").ToString
                objHistoriaBasica.ImpresionDiagnostica.HabilitarTiempoAntiguedadOcupacion = objHistoriaBasica.ImpresionDiagnostica.HabilitarSospechaATEP
                Valores(6) = dsDatosID.Tables(2).Rows(0)("unimedAntigue").ToString
                objHistoriaBasica.ImpresionDiagnostica.HabilitarAntiguedadOcupacion = objHistoriaBasica.ImpresionDiagnostica.HabilitarSospechaATEP
            Else
                optSospechaATEPNO.Tag = String.Empty
                optSospechaATEPSI.Tag = String.Empty

                optSospechaATEPNO.Enabled = True
                optSospechaATEPSI.Enabled = True

                optSospechaATEPNO.Checked = False
                optSospechaATEPSI.Checked = False
                Valores(3) = ""
                Valores(4) = ""
                Valores(5) = 0
                Valores(6) = ""
            End If

            If dsDatosID.Tables(3).Rows.Count > 0 Then
                Valores(7) = dsDatosID.Tables(3).Rows(0)("SospechaReaccion").ToString
                If Valores(7).ToString = "S" Then
                    objHistoriaBasica.ImpresionDiagnostica.HabilitarSospechaRAM = puedeModificarImpresionDiagn()
                End If
                Valores(8) = dsDatosID.Tables(3).Rows(0)("Ampliacion").ToString
                If Valores(8).ToString.Length > 0 Then
                    objHistoriaBasica.ImpresionDiagnostica.HabilitarReaccionAdversa = puedeModificarImpresionDiagn()
                End If
            Else
                Valores(7) = ""
                Valores(8) = ""
            End If
        End If

        ActualizarInformacionObjetoImpresionDiagnostica(Valores)
        ActualizarInformacionImpresionDiagnostica()
        ActualizarInformacionControlesID()
        dsDatosID = Nothing
