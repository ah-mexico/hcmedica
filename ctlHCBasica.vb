                        txtOtroComRN.Text = dtDatosAPE.Rows(0)("OtraComplicacionRN").ToString()
                        txtOtroComParto.Text = dtDatosAPE.Rows(0)("OtraComplicacionParto").ToString()

                    End If


                    If dsDatosAPE.Tables(1).Rows.Count > 0 Then

                        For Each dr As DataRow In dsDatosAPE.Tables(1).Rows

                            For i As Integer = 0 To chlComplicacionesRN.Items.Count - 1
                                RV = chlComplicacionesRN.Items.Item(i)

                                If dr.Item("IdComplicacion") = RV.Row.Item("IdComplicacion") Then
                                    chlComplicacionesRN.SetItemChecked(i, True)
                                    If dr.Item("IdComplicacion") = 10 Then
                                        lblOtroComRN.Visible = True
                                        txtOtroComRN.Visible = True
                                    End If
                                End If

                            Next
                        Next

                    End If

                    If dsDatosAPE.Tables(2).Rows.Count > 0 Then
                        For Each dr As DataRow In dsDatosAPE.Tables(2).Rows

                            For i As Integer = 0 To chlComplicacionesParto.Items.Count - 1
                                RV = chlComplicacionesParto.Items.Item(i)

