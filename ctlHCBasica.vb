    Private Sub BtnAgregarTransf_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles BtnAgregarTransf.Click
        Try

            If ValidarCamposObligatorios(pnlDatosAntTransfusionales) = False Then
                MessageBox.Show("Falta Información en Campo Obligatorio")
                Exit Sub
            Else
                If chlComponente.CheckedItems.Count = 0 Then
                    MessageBox.Show("Debe seleccionar por lo menos un Componente.")
                    chlComponente.Focus()
                    Exit Sub
                End If
                If RbtnSiReaccionTransf.Checked = False And RbtnNoReaccionTransf.Checked = False And RbtnNoRecuerdaReaccionTransf.Checked = False Then
                    MessageBox.Show("Debe Indicar si tuvo o no reacción adversa")
                    RbtnSiReaccionTransf.Focus()
                    Exit Sub
                End If
            End If

            If ValidoMesAnno(txtIdMesTransf.Text, txtAnoTransf.Text) = 2 Then
                MessageBox.Show("Debe validar los datos registrados")
                txtIdMesTransf.Focus()
                Exit Sub
            End If


            If strDatoModificado.Trim.Length = 0 Then
                AdicionarNuevoAntecedenteTransFusional()
            Else
                ModificarAntecedenteTransFusional(dtgAntecedenteTransfusional.CurrentRow, sender, String.Empty)
            End If
            strDatoModificado = ""
            ErrorAntecedenteModulo = ""
            RbtnOpcSiTransf.Checked = True
            pnlRecibeTransfusion.Enabled = False
        Catch ex As Exception
            MessageBox.Show("No fue posible adicionar la información ingresada.")

        End Try
    End Sub

    Private Sub dtgAntecedenteTransfusional_CellContentClick(ByVal sender As System.Object, ByVal e As System.Windows.Forms.DataGridViewCellEventArgs) Handles dtgAntecedenteTransfusional.CellContentClick
        Try
            If e.RowIndex = -1 Then
                Return
            End If

            If dtgAntecedenteTransfusional.Rows(e.RowIndex).Cells("EditableATF").Value = 0 Then
                If dtgAntecedenteTransfusional.Columns(e.ColumnIndex).Name = "ConfirmaATF" Then

                    Dim row As DataGridViewRow = dtgAntecedenteTransfusional.Rows(e.RowIndex)

                    Dim cellSelecion As DataGridViewCheckBoxCell = TryCast(row.Cells("NiegaATF"), DataGridViewCheckBoxCell)

                    If Convert.ToBoolean(cellSelecion.Value) Then
                        row.Cells("NiegaATF").Value = 0
                    End If

                Else
                    If dtgAntecedenteTransfusional.Columns(e.ColumnIndex).Name = "NiegaATF" Then

                        Dim row As DataGridViewRow = dtgAntecedenteTransfusional.Rows(e.RowIndex)

                        Dim cellSelecion As DataGridViewCheckBoxCell = TryCast(row.Cells("ConfirmaATF"), DataGridViewCheckBoxCell)

                        If Convert.ToBoolean(cellSelecion.Value) Then
                            row.Cells("ConfirmaATF").Value = 0
                        End If

                    Else
                        Return
                    End If
                End If

                dtgAntecedenteTransfusional.CommitEdit(DataGridViewDataErrorContexts.Commit)
                'Else
                'MessageBox.Show("Este antecedente no puede ser negado ya que acaba de ser ingresado")
            End If
