    Private Sub ValidarFinalizarHcb()
        Dim objFinalizarHcb As New BLHistoriaBasica

        Dim Datos() As Object
        Dim lExistencia As Long
        Dim dtExistencia As DataTable

        ReDim Datos(4)
        Datos(0) = objDatosGenerales.Prestador
        Datos(1) = objDatosGenerales.Sucursal
        Datos(2) = objDatosPaciente.TipoAdmision
        Datos(3) = objDatosPaciente.AnoAdmision
        Datos(4) = objDatosPaciente.NumeroAdmision

        Try

            dtExistencia = objFinalizarHcb.NVALIDAGRABACIONFINALHcb(objConexion, lExistencia, Datos)
            If dtExistencia.Rows.Count > 0 Then
                MarcacionNoRefiereAntecedentes()
                BloquearRegistroFinalizado()
            Else
                DesMarcacionNoRefiereAntecedentes()
                DesbloquearRegistro()
            End If
        Catch ex As Exception
            MessageBox.Show("Error Validacion Finalizar Historia Basica")
        End Try
    End Sub

    Private Sub MarcacionNoRefiereAntecedentes()
        Dim objFinalizarHcb As New BLHistoriaBasica
