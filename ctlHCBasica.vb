    Private Sub AdicionarNuevoAntecedenteTransFusional()
        Dim objActualizarAP As New BLHistoriaBasica
        Dim DatosTF() As Object
        Dim DatosTFC() As Object
        Dim IdAntTransfusional As Long '= 0
        Dim IdComponente As String = String.Empty
        Dim Componente As String = String.Empty
        Dim dtRows As DataRow()
        Dim dtRow As DataRow
        Dim lError As Long

        If dtgAntecedenteTransfusional.DataSource.Rows.Count > 0 Then
            'dtRows = dtgAntecedenteTransfusional.DataSource.Select("tiempomes=" & txtIdMesTransf.Text.ToString & _
            '        " AND tiempoano=" & txtAnoTransf.Text.ToString & _
            '        " AND IdComponente=" & txtIdComponente.Text.ToString)
            'If dtRows.Length > 0 Then
            '    MessageBox.Show("Registro Ya Existe")
            '    Exit Sub
            'End If
            'dtRows = dtgAntecedenteTransfusional.DataSource.Select("IdComponente='" & txtIdComponente.Text.ToString & "'")
            'If dtRows.Length > 0 Then
            '    If MessageBox.Show("Este Complemento ya existe en los Antecedentes. Desea registrarlo nuevamente?", "Antecedentes Transfusionales", MessageBoxButtons.YesNo) = DialogResult.No Then
            '        Exit Sub
            '    End If
            'End If
        End If

        ReDim DatosTF(16)
        DatosTF(0) = objDatosGenerales.Prestador
        DatosTF(1) = objDatosGenerales.Sucursal
        DatosTF(2) = objDatosPaciente.TipoDocumento
        DatosTF(3) = objDatosPaciente.NumeroDocumento
        DatosTF(4) = objDatosPaciente.TipoAdmision
        DatosTF(5) = objDatosPaciente.AnoAdmision
        DatosTF(6) = objDatosPaciente.NumeroAdmision
        'Datos(7) = txtIdComponente.Text.ToString
        DatosTF(7) = txtOtroComponente.Text.ToString
        DatosTF(8) = IIf(RbtnSiReaccionTransf.Checked, txtDescAdversa.Text.ToString, IIf(RbtnNoReaccionTransf.Checked, RbtnNoReaccionTransf.Text.ToString, RbtnNoRecuerdaReaccionTransf.Text.ToString))

        If txtIdMesTransf.Text.Trim.Length = 0 Then
            DatosTF(9) = DBNull.Value
        ElseIf txtIdMesTransf.Text.Trim.Length > 0 Then
            DatosTF(9) = txtIdMesTransf.Text
        End If

        If txtAnoTransf.Text.Trim.Length = 0 Then
            DatosTF(10) = DBNull.Value
        ElseIf txtAnoTransf.Text.Trim.Length > 0 Then
            DatosTF(10) = txtAnoTransf.Text
        End If

        DatosTF(11) = 1 'confirma
        DatosTF(12) = 0 'Niega
        DatosTF(13) = objDatosGenerales.Login
        DatosTF(14) = IdAntTransfusional
        DatosTF(15) = objDatosGenerales.CodigoEspecialidad
        DatosTF(16) = lError                              '@lError

        Try

            ErrorAntecedenteModulo = "AdicionarNuevoAntecedenteTransFusional"

            IdAntTransfusional = objActualizarAP.ActualizarAntecedenteTransfusional(objConexion, IdAntTransfusional, DatosTF)
            If IdAntTransfusional = 0 Then
                MessageBox.Show("Error al grabar datos Antecedentes Transfusional")
            Else
                IdEncTransfusion = IdAntTransfusional
                'ReDim Datos(0)
                'Datos(0) = IdAntTransfusional
                'IdAntTransfusional = objActualizarAP.EliminaAntecedenteTransFusionalComponente(objConexion, IdAntTransfusional, Datos)
                If chlComponente.CheckedItems.Count <> 0 Then

                    ReDim DatosTFC(2)
                    DatosTFC(0) = IdAntTransfusional
                    DatosTFC(2) = lError                              '@lError
                    Dim x As Integer
                    Dim s As String = ""
                    For x = 0 To chlComponente.CheckedItems.Count - 1
                        DatosTFC(1) = CType(chlComponente.CheckedItems(x), DataRowView)("IdComponente")
                        lError = objActualizarAP.ActualizarAntecedenteTransfusional(objConexion, IdAntTransfusional, DatosTFC)
                        If lError = 0 Then
                            If DatosTFC(1) = 8 Then
                                Componente += "Otro(" + txtOtroComponente.Text.ToString + "), "
                            Else
                                Componente += CType(chlComponente.CheckedItems(x), DataRowView)("Descripcion").ToString + ", "
                            End If
                            IdComponente += CType(chlComponente.CheckedItems(x), DataRowView)("IdComponente").ToString + ", "
                        End If
                    Next x
                End If

                objDatosGenerales.HistoriaClinicaTieneModificaciones = True
                dtRow = dtgAntecedenteTransfusional.DataSource.NewRow()
                With dtRow
                    .Item("cod_pre_sgs") = DatosTF(0)
                    .Item("cod_sucur") = DatosTF(1)
                    .Item("Confirma") = DatosTF(11)
                    .Item("Niega") = DatosTF(12)
                    .Item("tip_doc") = DatosTF(2)
                    .Item("num_doc") = DatosTF(3)
                    .Item("tip_admision") = DatosTF(4)
                    .Item("ano_adm") = DatosTF(5)
                    .Item("num_adm") = DatosTF(6)
                    .Item("Idcomponente") = IdComponente.Substring(0, IdComponente.Length - 1)
                    .Item("Componente") = Componente.Substring(0, Componente.Length - 1)
                    .Item("IdAntTransfusional") = IdEncTransfusion
                    .Item("OtroComponente") = DatosTF(7)
                    .Item("ReaccionAdversa") = DatosTF(8)
                    .Item("tiempomes") = DatosTF(9)
                    .Item("tiempoano") = DatosTF(10)
                    '.Item("confidencial") = Datos(14)
                    .Item("Editable") = 1
                    .Item("Login") = objDatosGenerales.Login
                End With

                dtgAntecedenteTransfusional.DataSource.Rows.Add(dtRow)
                dtgAntecedenteTransfusional.Height = dtgAntecedenteTransfusional.ColumnHeadersHeight + (dtgAntecedenteTransfusional.Rows.GetRowsHeight(DataGridViewElementStates.None))
                LimpiarPantallaAntecedentesTransfusionales()
                DirectCast(dtgAntecedenteTransfusional.Rows(dtgAntecedenteTransfusional.RowCount - 1).Cells("NiegaATF"), DataGridViewCheckBoxCell).ReadOnly = True
                DirectCast(dtgAntecedenteTransfusional.Rows(dtgAntecedenteTransfusional.RowCount - 1).Cells("ConfirmaATF"), DataGridViewCheckBoxCell).ReadOnly = True

            End If
        Catch ex As Exception
            ControlAntecedentesHb(ex.Source & " - " & Environment.MachineName.ToString & " - " & ErrorAntecedenteModulo, ex.Message, ex.StackTrace & " - " & ex.TargetSite.ToString)
            MessageBox.Show("Error al Grabar Datos Antecedentes Transfusional")
        End Try

    End Sub

