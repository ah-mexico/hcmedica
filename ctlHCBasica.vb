    'Proyecto: Sophia - Conciliación Medicamentos (VISUALIZAR CONCILIACIÓN DE MEDICAMENTOS EN ANTECEDENTES FARMACOLOGICOS CU_CONC_01)
    'Cambio: Método para traer los campos del select cuando se quiere modificar un registro de un medicamento.
    Private Sub ActivarEditarAFar(ByVal dr As DataGridViewRow)

        Dim dtcmbUM As New DataTable
        Dim dtcmbVA As New DataTable
        Dim dtcmbFRE As New DataTable
        Dim rows() As DataRow

        dtcmbUM = cmbUnidadMedidaAFar.DataSource
        rows = dtcmbUM.Select("concentracion='" & dr.Cells("UniMedDosis").Value.ToString & "'")
        If rows.Length = 0 Then
            cmbUnidadMedidaAFar.Text = ""
        Else
            If Not IsDBNull(rows(0).Item("descripcion").ToString) Then
                cmbUnidadMedidaAFar.Text = rows(0).Item("descripcion").ToString
            End If
        End If

        dtcmbVA = cmbViaAdministracionAFar.DataSource
        rows = dtcmbVA.Select("via='" & dr.Cells("ViaAdministra").Value.ToString & "'")
        If rows.Length = 0 Then
            cmbViaAdministracionAFar.Text = ""
        Else
            If Not IsDBNull(rows(0).Item("descripcion").ToString) Then
                cmbViaAdministracionAFar.Text = rows(0).Item("descripcion").ToString
            End If
        End If

        dtcmbFRE = cmbFrecuenciaAFar.DataSource
        rows = dtcmbFRE.Select("frecuencia='" & dr.Cells("Frecuencia").Value.ToString & "'")
        If rows.Length = 0 Then
            cmbFrecuenciaAFar.Text = ""
        Else
            If Not IsDBNull(rows(0).Item("descripcion").ToString) Then
                cmbFrecuenciaAFar.Text = rows(0).Item("descripcion").ToString
            End If
        End If

        txtDosisAFar.Text = dr.Cells("dosis").Value

    End Sub

