    Private Sub MostrarInformacionMC()
        Dim dtDatosMC As DataTable
        Dim dtDatosRevSistemas As DataTable

        If Not dsDatosMC Is Nothing And dsDatosMC.Tables.Count > 0 Then
            dtDatosMC = dsDatosMC.Tables(0)
            If Not dtDatosMC Is Nothing And dtDatosMC.Rows.Count > 0 Then
                habilitarModificacionMotivoConsulta(dtDatosMC)
                ActualizarInformacionObjetoMotivoConsulta(dtDatosMC.Rows(0)("Inf_SumPor").ToString, dtDatosMC.Rows(0)("mot_consulta").ToString, dtDatosMC.Rows(0)("Enf_actual").ToString)
            End If
        End If
        If dsDatosRevisionXSistema.Rows.Count > 0 Then

            dtDatosRevSistemas = dsDatosRevisionXSistema
            dtDatosRevSistemas.Columns.Add("copiaobs", System.Type.GetType("System.String"))
            'dtDatosRevSistemas.Columns.Add("copiaRefiere", System.Type.GetType("System.String"))
            'dtDatosRevSistemas.DefaultView.Sort = "DESCRIPCION"
            objHistoriaBasica.MotivoDeConsulta.RevisionPorSistemasMotivoConsulta = dtDatosRevSistemas
            ActualizarInformacionMotivoConsulta()
            ActualizarInformacionControles()

        End If
        dsDatosMC = Nothing
        dtDatosRevSistemas = Nothing
    End Sub

    Private Sub MostrarInformacionAPE()
        Dim RV As DataRowView

        If (objDatosPaciente.CodigoUnidadMedidaEdad = "A" And objDatosPaciente.Edad < 6) Or (objDatosPaciente.CodigoUnidadMedidaEdad = "D" Or objDatosPaciente.CodigoUnidadMedidaEdad = "M") Then
            pnlTitAntPerinatales.Visible = True
            pnlDatosAntecedentesPerinatales.Visible = False
            frmHistoriaPrincipal.lnkAntPerinatales.Visible = True

            If dsDatosAPE IsNot Nothing Then

                'limpiar data registro anterior
                txtEdadGestacional.ResetText()
                txtPesoAPE.ResetText()
                txtPCefalicoAPE.ResetText()
                txtTallaAPE.ResetText()
                txtPAbdominalAPE.ResetText()
                txtOtroComParto.ResetText()
                txtOtroComRN.ResetText()

                rbtUnica.Checked = False
                rbtMultiple.Checked = False

                LimpiarComponentesTipoParto()
                LimpiarComponentesCompParto()
                LimpiarComponentesCompRn()

                chlTipoParto.ClearSelected()
                chlComplicacionesParto.ClearSelected()
                chlComplicacionesRN.ClearSelected()

                If dsDatosAPE.Tables(0).Rows.Count > 0 Then

                    If dtDatosAPE.Rows.Count > 0 Then
                        cmdAntPerinatal.Image = My.Resources.Collapse
                        lblInfoAntPerinatal.Text = dtDatosAPE.Rows.Count
                        lblInfoAntPerinatal.Visible = True

                        txtEdadGestacional.Text = dtDatosAPE.Rows(0)("EdadGestacional").ToString()
                        Select Case dtDatosAPE.Rows(0)("TipoGestacion").ToString()
                            Case "1"
                                rbtUnica.Checked = True
                            Case "2"
                                rbtMultiple.Checked = True
                        End Select

                        If Val(dtDatosAPE.Rows(0)("peso").ToString()) = 0 Then
                            txtPesoAPE.Text = ""
                        Else
                            txtPesoAPE.Text = Val(dtDatosAPE.Rows(0)("peso").ToString())
                        End If

                        If Val(dtDatosAPE.Rows(0)("talla").ToString()) = 0 Then
                            txtTallaAPE.Text = ""
                        Else
                            txtTallaAPE.Text = Val(dtDatosAPE.Rows(0)("talla").ToString())
                        End If

                        If Val(dtDatosAPE.Rows(0)("P_Cefalico").ToString()) = 0 Then
                            txtPCefalicoAPE.Text = ""
                        Else
                            txtPCefalicoAPE.Text = Val(dtDatosAPE.Rows(0)("P_Cefalico").ToString())
                        End If

                        If Val(dtDatosAPE.Rows(0)("P_Abdominal").ToString()) = 0 Then
                            txtPAbdominalAPE.Text = ""
