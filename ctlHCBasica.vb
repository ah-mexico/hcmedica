    Private Sub CambiaEstadoCondicionEntidadPaliativa()
        '''el sistema debe generar el siguiente mensaje emergente al finalizar el diligenciamiento de la historia clínica básica: 
        '''''"¿El motivo de consulta del Paciente (nombre completo del paciente) está relacionado con su condición paliativa?" 
        ''(Con dos opciones de respuesta "SI" y "NO"). Si la respuesta seleccionada es "SI", el sistema debe desaparecer el mensaje emergente; 
        ''de lo contrario, si la respuesta es "NO" el sistema debe cambiar la entidad de Cuidados Paliativos en la admisión tipo U.
        Dim strRespuesta As String = ""
        Dim lError As Long
        Dim DlRespuesta As DialogResult
        Dim strOpcionCondicionPaliativa As String = ""

        ''Cuando el Paciente pertenece al Programa de Cuidados Paliativos (Si La Hoja esta activa)
        If Me.ParentForm.Controls.Find("pnlContenedorDatosPaciente", True)(0).Controls(0).Controls.Find("pnlAlarmas", True)(0).Controls.Find("btnPalitivo", True)(0).Visible = True Then
            DlRespuesta = MessageBox.Show("¿El motivo de consulta del Paciente " + objDatosPaciente.NombreCompleto.ToString + " está relacionado con su condición paliativa? ", "Historia Básica", MessageBoxButtons.YesNo, MessageBoxIcon.Information)
            '''Si la respuesta es "NO" el sistema debe cambiar la entidad de Cuidados Paliativos en la admisión tipo U.
            If DlRespuesta = DialogResult.No Then
                strOpcionCondicionPaliativa = "N"
            Else
                strOpcionCondicionPaliativa = "S"
            End If
            Try
                Dim objCambiarEntidadPaliativa As New BLHistoriaBasica
                Dim Datos() As Object
                ReDim Datos(9)
                Datos(0) = objDatosPaciente.TipoDocumento
                Datos(1) = objDatosPaciente.NumeroDocumento
                Datos(2) = objDatosGenerales.Prestador
                Datos(3) = objDatosGenerales.Sucursal
                Datos(4) = objDatosPaciente.TipoAdmision
                Datos(5) = objDatosPaciente.AnoAdmision
                Datos(6) = objDatosPaciente.NumeroAdmision
                Datos(7) = strOpcionCondicionPaliativa
                Datos(8) = objDatosGenerales.Login
                Datos(9) = lError
                lError = objCambiarEntidadPaliativa.ActualizarEntidadCuidadoPaliativo(objConexion, lError, Datos)

            Catch ex As Exception
                MessageBox.Show("Error al marcar la Condicion Paliativa: " + ex.Message)
            End Try

        End If
    End Sub ''' WACHV, FIN, 12OCT2017, Requerimiento 2. REQ.F02 Condicion paliativa


