    Private Function ValidarCamposObligatoriosDatosVacunas() As Boolean
        Dim intReg As Integer
        Dim intCol As Integer

        For intReg = 0 To dtgVacunas.Rows.Count - 1
            For intCol = 0 To dtgVacunas.Columns.Count - 1
                If dtgVacunas.Rows(intReg).Cells(intCol).ErrorText = "Dato Obligatorio" Then
                    dtgVacunas.Rows(intReg).Cells(intCol).Selected = True
                    Return False
                End If
            Next
        Next

        Return True

    End Function

