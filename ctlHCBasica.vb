    Private Sub ActualizarInformacionObjetoExamenFisico(ByVal ParamArray Valores() As Object)
        With objHistoriaBasica.ExamenFisico


            If Valores(0) Is DBNull.Value Then
                .TensionArterialSistole = Nothing
            Else
                .TensionArterialSistole = Convert.ToInt32(Valores(0))
            End If

            If Valores(1) Is DBNull.Value Then
                .TensionArterialDiastole = Nothing
            Else
                .TensionArterialDiastole = Convert.ToInt32(Valores(1))
            End If

            .FrecuenciaRespiratoria = ReemplazarString(Valores(2), 0, "0")
            .FrecuenciaCardiaca = ReemplazarString(Valores(3), 0, "0")
            .Temperatura = ReemplazarString(Valores(4), 0, "0")

            If Valores(5) Is DBNull.Value Then
                .Peso = Nothing
            Else
                .Peso = Convert.ToSingle(Valores(5))
            End If

            If Valores(6) Is DBNull.Value Then
                .Talla = Nothing
            Else
                .Talla = Convert.ToSingle(Valores(6))
            End If

            If Valores(7) Is DBNull.Value Then
                .IndiceDeMasaCorporal = Nothing
            Else
                .IndiceDeMasaCorporal = ReemplazarString(Valores(7), 0, "0")
            End If

            If Valores(8) Is DBNull.Value Then
                .PerimetroCefalico = Nothing
            Else
                .PerimetroCefalico = Convert.ToSingle(Valores(8))
            End If

            If Valores(9) Is DBNull.Value Then
                .PerimetroAbdominal = Nothing
            Else
                .PerimetroAbdominal = Convert.ToSingle(Valores(9))
            End If

            If Valores(10) Is DBNull.Value Then
                .Glasgow = Nothing
            Else
                .Glasgow = ReemplazarString(Valores(10), 0, "0")
            End If

            .Embriaguez = ReemplazarString(Valores(11), DBNull.Value, "")
            .CodigoEstadoDeConciencia = ReemplazarString(Valores(12), DBNull.Value, "")
            .EstadoGeneral = ReemplazarString(Valores(13), DBNull.Value, "")
            .Confidencial = ReemplazarString(Valores(14), DBNull.Value, "")
            .SaturacionOxigeno = ReemplazarString(Valores(15), 0, "0")
            .EAnaloga_dolor = ReemplazarString(Valores(16), DBNull.Value, "")

        End With
    End Sub

    Private Sub ActualizarInformacionExamenFisico()

        With objHistoriaBasica.ExamenFisico

            If dsDatosEF.Tables(0).Rows.Count > 0 Then

                If IsNothing(.TensionArterialSistole) Then
                    txtSistoleEF.Text = ""
                Else
                    txtSistoleEF.Text = ReemplazarString(.TensionArterialSistole, 0, "0")
                End If

                If IsNothing(.TensionArterialDiastole) Then
                    txtDiastoleEF.Text = ""
                Else
                    txtDiastoleEF.Text = ReemplazarString(.TensionArterialDiastole, 0, "0")
                End If

                txtFrecuenciaRespiratoriaEF.Text = ReemplazarString(.FrecuenciaRespiratoria, 0, "0")
                txtFrecuenciaCardiacaEF.Text = ReemplazarString(.FrecuenciaCardiaca, 0, "0")
                txtTemperaturaEF.Text = ReemplazarString(.Temperatura, 0, "0")

                If IsNothing(.Peso) Then
