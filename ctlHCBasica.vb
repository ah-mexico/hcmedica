                If Not String.IsNullOrEmpty(dtgRevisionSistemasMC.Rows(intDato).Cells("NoRefiere").Value.ToString) Then

                    If dtgRevisionSistemasMC.Rows(intDato).Cells("NoRefiere").Value = 1 Or dtgRevisionSistemasMC.Rows(intDato).Cells("NoRefiere").Value = True Then

                        Datos2(7) = dtgRevisionSistemasMC.Rows(intDato).Cells("IdSistema").Value.ToString
                        Datos2(8) = String.Empty
                        Datos2(9) = 0

