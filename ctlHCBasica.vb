            Datos(7) = objDatosGenerales.Login
            Datos(8) = lError

            If EsqCompleto = 1 Then
                Datos(5) = EsqCompleto
                Datos(6) = ""
            ElseIf EsqCompleto = 0 Then
                Datos(5) = EsqCompleto

                If Observacion.Trim.Length = 0 Then
                    Datos(6) = ""
                ElseIf Observacion.Trim.Length > 0 Then
                    Datos(6) = Observacion
                End If

            End If

            lError = objActualizarAP.ActualizarEsquemaVacunacion(objConexion, lError, Datos)

