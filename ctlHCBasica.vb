    Private Sub validatingYearControl(ByVal sender As Object, ByVal e As System.ComponentModel.CancelEventArgs)

        Dim txtBoxMonth As TextBox = Nothing

        If (sender.Equals(Me.txtYearUltimMamograf)) Then
            txtBoxMonth = Me.txtMesUltimMamograf
        ElseIf (sender.Equals(Me.txtYearUltimCitolog)) Then
            txtBoxMonth = Me.txtMesUltimCitolog
        End If

        Dim errorMessage As String = Nothing
        If (Not txtBoxMonth Is Nothing) Then
            If (Not txtBoxMonth.Text = String.Empty And CType(sender, TextBox).Text = String.Empty) Then
                'MessageBox.Show("El campo del año es requerido.", "Antecedentes Ginecobstetricos", MessageBoxButtons.OK, MessageBoxIcon.Stop)
                'e.Cancel = True
            Else
                Dim txtBoxYear As TextBox = CType(sender, TextBox)
                If (txtBoxYear.Text <> String.Empty) Then

                    If objDatosPaciente.CodigoUnidadMedidaEdad = "A" Then

                        If (Val(txtBoxYear.Text) > (Now.Year - objDatosPaciente.Edad)) Then
                            If (Val(txtBoxYear.Text) < Now.Year) Or (Val(txtBoxMonth.Text) <= Now.Month And (Val(txtBoxYear.Text) = Now.Year)) Then
                                Exit Sub
                            End If
                            errorMessage = "Mes o año invalido. Verifique que el mes y año sean menores o iguales a los que corresponden a la fecha actual."
                        Else
                            errorMessage = "El año no puede ser menor al año " & (Now.Year - objDatosPaciente.Edad)
                        End If

                        If Not txtBoxYear Is Nothing Then
                            txtBoxYear.Clear()
                        End If

                    End If

                End If
            End If
            If Not errorMessage Is Nothing Then
                MessageBox.Show(errorMessage, "Antecedentes Gineco-Obstetricos", MessageBoxButtons.OK, MessageBoxIcon.Information)
                e.Cancel = True
            End If
        End If
    End Sub

    ''' <summary>
    ''' Handler que hace la validacion del mes.
    ''' </summary>
    ''' <param name="sender"></param>
    ''' <param name="e"></param>
    ''' <remarks></remarks>
    Private Sub txtMesUltimCitolog_Validating(ByVal sender As Object, ByVal e As System.ComponentModel.CancelEventArgs)


        Dim txtBoxYear As TextBox = Nothing

        If (sender.Equals(Me.txtMesUltimMamograf)) Then
            txtBoxYear = Me.txtYearUltimMamograf
        ElseIf (sender.Equals(Me.txtMesUltimCitolog)) Then
            txtBoxYear = Me.txtYearUltimCitolog
        End If

        Dim errorMessage As String = Nothing
        If (Not txtBoxYear Is Nothing) Then
            If (txtBoxYear.Text <> String.Empty And CType(sender, TextBox).Text <> String.Empty) Then
                Dim txtBoxMonth As TextBox = CType(sender, TextBox)

                If (Val(txtBoxYear.Text) > (Now.Year - 100)) Then
                    If (Val(txtBoxYear.Text) < Now.Year) Or (Val(txtBoxMonth.Text) <= Now.Month And (Val(txtBoxYear.Text) = Now.Year)) Then
                        Exit Sub
                    End If
                    errorMessage = "Mes o año invalido. Verifique que el mes y año sean menores o iguales a los que corresponden a la fecha actual."
                Else
                    errorMessage = "El año no puede ser menor al año " & (Now.Year - 100)
                End If

                If Not txtBoxMonth Is Nothing Then
                    txtBoxMonth.Clear()
                End If
            End If
            If Not errorMessage Is Nothing Then
                MessageBox.Show(errorMessage, "Antecedentes Gineco-Obstetricos", MessageBoxButtons.OK, MessageBoxIcon.Information)
                e.Cancel = True
            End If
        End If
    End Sub


