    Private Sub mskValidarFecha(ByVal sender As Object, ByVal e As System.ComponentModel.CancelEventArgs) Handles mskFechaUltimoParto.Validating, mskFechaUltimaMestruacion.Validating '' '', mskFechaUltimaCitologia.Validating
        If sender.Text <> "  /  /" Then
            If Not IsDate(sender.Text) Then
                MessageBox.Show("Debe validar los datos registrados", "Antecedentes Ginecológicos", MessageBoxButtons.OK, MessageBoxIcon.Information)
                e.Cancel = True
                sender.Select(0, sender.TextLength)
            Else
                If CDate(sender.text) > CDate(Format(Now, "dd/MM/yyyy")) Then
                    MessageBox.Show("Debe validar los datos registrados", "Antecedentes Ginecológicos", MessageBoxButtons.OK, MessageBoxIcon.Information)
                    e.Cancel = True
                    sender.Select(0, sender.TextLength)
                End If

                If CDate(sender.text) < objDatosPaciente.FechaNacimiento Then
                    MessageBox.Show("Debe validar los datos registrados", "Antecedentes Ginecológicos", MessageBoxButtons.OK, MessageBoxIcon.Information)
                    e.Cancel = True
                    sender.Select(0, sender.TextLength)
                End If

            End If
        End If
    End Sub

