    Private Sub AdicionarNuevoAntecedenteQuirurgico()
        Dim objActualizarAP As New BLHistoriaBasica
        Dim Datos() As Object
        Dim lError As Long
        Dim dtRows As DataRow()
        Dim dtRow As DataRow

        If dtgAntecedentesQuirurgicos.DataSource.Rows.Count > 0 Then
            dtRows = dtgAntecedentesQuirurgicos.DataSource.Select("tip_admision='" & objDatosPaciente.TipoAdmision & "' AND ano_adm=" & objDatosPaciente.AnoAdmision & " AND num_adm=" & objDatosPaciente.NumeroAdmision &
                         " AND procedim='" & txtCodDiagnosticoAQ.Text.ToString & "'")
            If dtRows.Length > 0 Then
                MessageBox.Show("Registro Ya Existe")
                Exit Sub
            End If
            dtRows = dtgAntecedentesQuirurgicos.DataSource.Select("procedim='" & txtCodDiagnosticoAQ.Text.ToString & "'")
            If dtRows.Length > 0 Then
                If MessageBox.Show("Este Procedimiento Ya Existe en los Antecedentes. Desea Registrarlo Nuevamente?", "Antecedentes Quirúrgicos", MessageBoxButtons.YesNo) = DialogResult.No Then
                    Exit Sub
                End If
            End If
        End If
        'Walter Prens Herrera 2019-11-22
        ReDim Datos(23)
        Datos(0) = objDatosGenerales.Prestador
        Datos(1) = objDatosGenerales.Sucursal
        Datos(2) = objDatosPaciente.TipoDocumento
        Datos(3) = objDatosPaciente.NumeroDocumento
        Datos(4) = objDatosPaciente.TipoAdmision
        Datos(5) = objDatosPaciente.AnoAdmision
        Datos(6) = objDatosPaciente.NumeroAdmision
        Datos(7) = txtCodDiagnosticoAQ.Text.ToString
        Datos(8) = ReemplazarString(Val(txtCodMesAQ.Text.ToString), 0, DBNull.Value)
        Datos(9) = ReemplazarString(Val(txtAnnoAQ.Text.ToString), 0, DBNull.Value)
        Datos(10) = objDatosGenerales.Login
        Datos(11) = txtObservacionesAQ.Text.ToString
        Datos(12) = 1 'confirma
        Datos(13) = 0 'Niega
        Datos(14) = objDatosGenerales.CodigoEspecialidad
        Datos(15) = 0 'Indica que no se migro de descripcion quirurgica
        Datos(16) = objDatosGenerales.Prestador
        Datos(17) = objDatosGenerales.Sucursal
        Datos(18) = objDatosPaciente.TipoAdmision
        Datos(19) = objDatosPaciente.AnoAdmision
        Datos(20) = objDatosPaciente.NumeroAdmision
        Datos(21) = ctluAntecQuirurgicos.XmlGrpXPar
        Datos(22) = ctluAntecQuirurgicos.XmlProcPar
        Datos(23) = lError

        Try
            ErrorAntecedenteModulo = "AdicionarNuevoAntecedenteQuirurgico"

            lError = objActualizarAP.ActualizarAntecedenteQuirurgico(objConexion, lError, Datos)
            If lError <> 0 Then
                MessageBox.Show("Error al Grabar Datos Antecedentes Quirurgicos")
            Else
                objDatosGenerales.HistoriaClinicaTieneModificaciones = True
                dtRow = dtgAntecedentesQuirurgicos.DataSource.NewRow()
                With dtRow
                    .Item("cod_pre_sgs") = Datos(0)
                    .Item("cod_sucur") = Datos(1)
                    .Item("Confirma") = Datos(12)
                    .Item("Niega") = Datos(13)
                    .Item("tip_doc") = Datos(2)
                    .Item("num_doc") = Datos(3)
                    .Item("tip_admision") = Datos(4)
                    .Item("ano_adm") = Datos(5)
                    .Item("num_adm") = Datos(6)
                    .Item("procedim") = Datos(7)
                    .Item("Descripcion") = cboDescProcedimientoAQ.Text.ToString
                    .Item("obs") = Datos(11)
                    .Item("tiempomes") = Datos(8)
                    .Item("tiempoano") = Datos(9)
                    '.Item("confidencial") = Datos(14)
                    .Item("medico") = objDatosGenerales.NombreMedico
                    .Item("Editable") = 1
                    .Item("Login") = objDatosGenerales.Login
                    .Item("Origen") = 1
                    .Item("MIGRADOQX") = Datos(15)
                    .Item("xmlGrupPar") = Datos(21)
                End With

                dtgAntecedentesQuirurgicos.DataSource.Rows.Add(dtRow)
                dtgAntecedentesQuirurgicos.Height = dtgAntecedentesQuirurgicos.ColumnHeadersHeight + (dtgAntecedentesQuirurgicos.Rows.GetRowsHeight(DataGridViewElementStates.None))
                LimpiarPantallaAntecedentesQuirurgicos()
                DirectCast(dtgAntecedentesQuirurgicos.Rows(dtgAntecedentesQuirurgicos.RowCount - 1).Cells("NiegaAQ"), DataGridViewCheckBoxCell).ReadOnly = True
                DirectCast(dtgAntecedentesQuirurgicos.Rows(dtgAntecedentesQuirurgicos.RowCount - 1).Cells("ConfirmaAQ"), DataGridViewCheckBoxCell).ReadOnly = True

                'cdqf
                If dtgAntecedentesQuirurgicos.RowCount > 0 Then
                    chkNoRefiereAntQuirurgico.Enabled = False
                Else
                    chkNoRefiereAntQuirurgico.Enabled = True
                End If

            End If
        Catch ex As Exception
            ControlAntecedentesHb(ex.Source & " - " & Environment.MachineName.ToString & " - " & ErrorAntecedenteModulo, ex.Message, ex.StackTrace & " - " & ex.TargetSite.ToString)
            MessageBox.Show("Error al Grabar Datos Antecedentes Quirurgicos")
        End Try

    End Sub

