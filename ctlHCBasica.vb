                If Not String.IsNullOrEmpty(dtgRevisionSistemasMC.Rows(intDato).Cells("Refiere").Value.ToString) Then

                    If dtgRevisionSistemasMC.Rows(intDato).Cells("Refiere").Value = 1 Or dtgRevisionSistemasMC.Rows(intDato).Cells("Refiere").Value = True Then
                        If dtgRevisionSistemasMC.Rows(intDato).Cells("Refiere").Value = 1 Then
