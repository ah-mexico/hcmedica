    Private Sub cmdAdicionarAG_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles cmdAdicionarAG.Click

        If txtMenarquiaAG.Text.Trim.Length = 0 Then
            MessageBox.Show("Falta Información en Campo Obligatorio")
            txtMenarquiaAG.Focus()
            Exit Sub
        End If

        If mskFechaUltimaMestruacion.Text = "  /  /" And chbNoRecuerdaFUM.Checked = False Then
            MsgBox("Falta Información en Campo Obligatorio", MsgBoxStyle.Information)
            mskFechaUltimaMestruacion.Focus()
            Exit Sub
        End If

        If mskFechaUltimaMestruacion.Text <> "  /  /" Then
            If Not IsDate(mskFechaUltimaMestruacion.Text) Then
                MsgBox("Debe validar los datos registrados", MsgBoxStyle.Information)
                mskFechaUltimaMestruacion.Focus()
                Exit Sub
                mskFechaUltimaMestruacion.Select(0, mskFechaUltimaMestruacion.TextLength)
            Else
                If CDate(mskFechaUltimaMestruacion.Text) > CDate(Format(Now, "dd/MM/yyyy")) Then
                    MsgBox("Debe validar los datos registrados", MsgBoxStyle.Information)
                    mskFechaUltimaMestruacion.Focus()
                    Exit Sub
                    mskFechaUltimaMestruacion.Select(0, mskFechaUltimaMestruacion.TextLength)
                End If
                If CDate(mskFechaUltimaMestruacion.Text) < objDatosPaciente.FechaNacimiento Then
                    MsgBox("Debe validar los datos registrados", MsgBoxStyle.Information)
                    mskFechaUltimaMestruacion.Focus()
                    Exit Sub
                    mskFechaUltimaMestruacion.Select(0, mskFechaUltimaMestruacion.TextLength)
                End If
            End If
        End If

        If txtMesUltimCitolog.Text.Trim.Length = 0 And CboMesUltimCitolog.Text.Trim.Length = 0 And txtYearUltimCitolog.Text.Trim.Length = 0 And chbNoRecuerdaCCV.Checked = False And chbNuncaCCV.Checked = False Then
            MessageBox.Show("Falta Información en Campo Obligatorio")
            txtMesUltimCitolog.Focus()
            Exit Sub
        End If

        If txtMesUltimCitolog.Text.Trim.Length > 0 And CboMesUltimCitolog.Text.Trim.Length > 0 Then
            If txtYearUltimCitolog.Text.Trim.Length = 0 Then
                MessageBox.Show("Falta Información en Campo Obligatorio", "Antecedentes Ginecobstetricos", MessageBoxButtons.OK, MessageBoxIcon.Stop)
                txtYearUltimCitolog.Focus()
                Exit Sub
            End If
        End If

        If txtYearUltimCitolog.Text.Trim.Length > 0 Then
            If txtMesUltimCitolog.Text.Trim.Length = 0 Or CboMesUltimCitolog.Text.Trim.Length = 0 Then
                MessageBox.Show("Falta Información en Campo Obligatorio", "Antecedentes Ginecobstetricos", MessageBoxButtons.OK, MessageBoxIcon.Stop)
                txtMesUltimCitolog.Focus()
                Exit Sub
            End If
        End If


        If ValidoMesAnno(txtMesUltimCitolog.Text, txtYearUltimCitolog.Text) = 2 Then
            MessageBox.Show("Debe validar los datos registrados")
            txtMesUltimCitolog.Focus()
            Exit Sub
        End If


        If txtMesUltimMamograf.Text.Trim.Length = 0 And CboMesUltimMamograf.Text.Trim.Length = 0 And txtYearUltimMamograf.Text.Trim.Length = 0 And chbNoRecuerdaMamografia.Checked = False And chbNuncaMamografia.Checked = False Then
            MessageBox.Show("Falta Información en Campo Obligatorio")
            txtMesUltimMamograf.Focus()
            Exit Sub
        End If

        If txtMesUltimMamograf.Text.Trim.Length > 0 And CboMesUltimMamograf.Text.Trim.Length > 0 Then
            If txtYearUltimMamograf.Text.Trim.Length = 0 Then
                MessageBox.Show("Falta Información en Campo Obligatorio", "Antecedentes Ginecobstetricos", MessageBoxButtons.OK, MessageBoxIcon.Stop)
                txtYearUltimMamograf.Focus()
                Exit Sub
            End If
        End If

        If txtYearUltimMamograf.Text.Trim.Length > 0 Then
            If txtMesUltimMamograf.Text.Trim.Length = 0 Or CboMesUltimMamograf.Text.Trim.Length = 0 Then
                MessageBox.Show("Falta Información en Campo Obligatorio", "Antecedentes Ginecobstetricos", MessageBoxButtons.OK, MessageBoxIcon.Stop)
                txtMesUltimMamograf.Focus()
                Exit Sub
            End If
        End If

        If ValidoMesAnno(txtMesUltimMamograf.Text, txtYearUltimMamograf.Text) = 2 Then
            MessageBox.Show("Debe validar los datos registrados")
            txtMesUltimMamograf.Focus()
            Exit Sub
        End If

        If ValidarCamposObligatorios(pnlDatosAntecedentesGinecologicos) = False Then
            MessageBox.Show("Falta Información en Campo Obligatorio")
            Exit Sub
        Else
            If chlMetodoPlanificacion.CheckedItems.Count = 0 Then
                MessageBox.Show("Debe seleccionar por lo menos un Método de Planificación.")
                Exit Sub
            End If
        End If

        If mskFechaUltimoParto.Text <> "  /  /" Then
            If Not IsDate(mskFechaUltimoParto.Text) Then
                MsgBox("Debe validar los datos registrados", MsgBoxStyle.Information)
                mskFechaUltimoParto.Focus()
                Exit Sub
                mskFechaUltimoParto.Select(0, mskFechaUltimoParto.TextLength)
            Else
                If CDate(mskFechaUltimoParto.Text) > CDate(Format(Now, "dd/MM/yyyy")) Then
                    MsgBox("Debe validar los datos registrados", MsgBoxStyle.Information)
                    mskFechaUltimoParto.Focus()
                    Exit Sub
                    mskFechaUltimoParto.Select(0, mskFechaUltimoParto.TextLength)
                End If

                If CDate(mskFechaUltimoParto.Text) < objDatosPaciente.FechaNacimiento Then
                    MsgBox("Debe validar los datos registrados", MsgBoxStyle.Information)
                    mskFechaUltimoParto.Focus()
                    Exit Sub
                    mskFechaUltimoParto.Select(0, mskFechaUltimoParto.TextLength)
                End If
            End If
        End If

        If strDatoModificado.Trim.Length = 0 Then
            AdicionarNuevoAntecedenteGinecologico()
        Else
            EliminarAntecedenteGinecologico(dtgAntecedentesGinecologicos.CurrentRow)
            ModificarAntecedenteGinecologico(dtgAntecedentesGinecologicos.CurrentRow)
        End If
        strDatoModificado = ""
        ErrorAntecedenteModulo = ""
        txtMenarquiaAG.Enabled = True
        txtMenarquiaAG.Focus()
    End Sub

