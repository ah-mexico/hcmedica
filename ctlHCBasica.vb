    Private Sub EditarDatosImpresionDiagnostica(ByVal dr As DataGridViewRow)
        txtCodTipoDiagnosticoID.Text = dr.Cells("tip_diagID").Value.ToString
        txtDescTipoDiagnosticoID.Text = txtCodTipoDiagnosticoID.DescripcionCodigo.ToString
        If txtCodCategoriaID.Visible = True Then
            If dr.Cells("categoria_diagID").Value.ToString.Trim.Length > 0 Then

                Select Case dr.Cells("categoria_diagID").Value.ToString
                    Case "INGRESO"
                        txtCodCategoriaID.Text = "I"
                        txtDescCategoriaID.Text = dr.Cells("categoria_diagID").Value.ToString
                    Case "RELACIONADO"
                        '"ASOCIADO"
                        txtCodCategoriaID.Text = "A"
