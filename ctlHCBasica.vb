    Private Sub AdicionarNuevoAntecedenteAlergico()
        Dim objActualizarAP As New BLHistoriaBasica
        Dim Datos() As Object
        Dim lError As Long
        Dim dtRows As DataRow()
        Dim dtRow As DataRow

        If dtgAntecedentesAlergicos.DataSource.Rows.Count > 0 Then

            dtRows = dtgAntecedentesAlergicos.DataSource.Select(" obs='" & txtAgenteCausal.Text.ToString & "' AND clase='" & txtCodClaseAA.Text.ToString & "'")
            If dtRows.Length > 0 Then
                MessageBox.Show("El Registro Ya Existe")
                Exit Sub
            End If

            dtRows = dtgAntecedentesAlergicos.DataSource.Select("clase='" & txtCodClaseAA.Text.ToString & "'")
            If dtRows.Length > 0 Then
                If MessageBox.Show("Este tipo de alergia ya existe. Desea agregarlo nuevamente?", "Antecedentes Alérgicos", MessageBoxButtons.YesNo) = DialogResult.No Then
                    Exit Sub
                End If
            End If
        End If

        ReDim Datos(20)
        Datos(0) = objDatosGenerales.Prestador
        Datos(1) = objDatosGenerales.Sucursal
        Datos(2) = objDatosPaciente.TipoDocumento
        Datos(3) = objDatosPaciente.NumeroDocumento
        Datos(4) = objDatosPaciente.TipoAdmision
        Datos(5) = objDatosPaciente.AnoAdmision
        Datos(6) = objDatosPaciente.NumeroAdmision
        Datos(7) = ReemplazarString(Val(txtCodClaseAA.Text.ToString), 0, DBNull.Value)
        Datos(8) = objDatosGenerales.Login
        Datos(9) = txtAgenteCausal.Text.ToString
        Datos(10) = ReemplazarString(Val(txtCodMesAA.Text.ToString), 0, DBNull.Value)
        Datos(11) = ReemplazarString(Val(txtAnnoAA.Text.ToString), 0, DBNull.Value)
        Datos(12) = 1 'confirma
        Datos(13) = 0 'Niega
        Datos(14) = objDatosGenerales.CodigoEspecialidad

        Datos(15) = objDatosGenerales.Prestador
        Datos(16) = objDatosGenerales.Sucursal
        Datos(17) = objDatosPaciente.TipoAdmision
        Datos(18) = objDatosPaciente.AnoAdmision
        Datos(19) = objDatosPaciente.NumeroAdmision

        Datos(20) = lError

        Try

            ErrorAntecedenteModulo = "AdicionarNuevoAntecedenteAlergico"

            lError = objActualizarAP.ActualizarAntecedenteAlergico(objConexion, lError, Datos)
            If lError <> 0 Then
                MessageBox.Show("Error al Grabar Datos Antecedentes Alergicos")
            Else
                objDatosGenerales.HistoriaClinicaTieneModificaciones = True
                dtRow = dtgAntecedentesAlergicos.DataSource.NewRow()
                With dtRow
                    .Item("cod_pre_sgs") = Datos(0)
                    .Item("cod_sucur") = Datos(1)
                    .Item("tip_doc") = Datos(2)
                    .Item("num_doc") = Datos(3)
                    .Item("tip_admision") = Datos(4)
                    .Item("ano_adm") = Datos(5)
                    .Item("num_adm") = Datos(6)
                    .Item("clase") = Datos(7)
                    .Item("descripcion") = txtDescClaseAA.Text.ToString
                    '.Item("alergico") = Datos(9)
                    '.Item("aDescripcion") = "OTROS"
                    .Item("obs") = Datos(9)
                    .Item("tiempomes") = Datos(10)
                    .Item("tiempoano") = Datos(11)
                    .Item("Confirma") = Datos(12)
                    .Item("Niega") = Datos(13)
                    '.Item("confidencial") = Datos(16)
                    .Item("Editable") = 1
                    .Item("medico") = objDatosGenerales.NombreMedico
                    .Item("EstadoUsu") = "N"
                End With
                dtgAntecedentesAlergicos.DataSource.Rows.Add(dtRow)
                dtgAntecedentesAlergicos.Height = dtgAntecedentesAlergicos.ColumnHeadersHeight + (dtgAntecedentesAlergicos.Rows.GetRowsHeight(DataGridViewElementStates.None))
                LimpiarPantallaAntecedentesAlergicos()
                DirectCast(dtgAntecedentesAlergicos.Rows(dtgAntecedentesAlergicos.RowCount - 1).Cells("NiegaAA"), DataGridViewCheckBoxCell).ReadOnly = True
                DirectCast(dtgAntecedentesAlergicos.Rows(dtgAntecedentesAlergicos.RowCount - 1).Cells("ConfirmaAA"), DataGridViewCheckBoxCell).ReadOnly = True

                'cdqf
                If dtgAntecedentesAlergicos.RowCount > 0 Then
                    chkNoRefiereAntAlergicos.Enabled = False
                Else
                    chkNoRefiereAntAlergicos.Enabled = True
                End If

            End If
        Catch ex As Exception
            ControlAntecedentesHb(ex.Source & " - " & Environment.MachineName.ToString & " - " & ErrorAntecedenteModulo, ex.Message, ex.StackTrace & " - " & ex.TargetSite.ToString)
            MessageBox.Show("Error al Grabar Datos Antecedentes Alergicos")
        End Try

    End Sub

