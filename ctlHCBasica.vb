        Try


            Dim dtDatosNRANT As DataTable = New DataTable()

            dtDatosNRANT = objFinalizarHcb.ConsultarListaNoRefiereAntecedentes(objConexion, lError, Datos)


            If dtDatosNRANT.Rows.Count > 0 Then

                For Each row As DataRow In dtDatosNRANT.Rows

