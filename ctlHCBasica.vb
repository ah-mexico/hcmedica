        Datos(8) = ""
        Datos(9) = ""
        Datos(10) = ""
        Datos(11) = objDatosGenerales.Login
        Datos(12) = lError

        Try
            dsDatosMC = objDatosMC.ConsultarDatosMotivoConsulta(objConexion, lError, Datos)

            If lError <> 0 Then
