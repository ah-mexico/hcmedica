                    RbtnNoVacunas.Checked = True

                    LbObsVacunas.Enabled = True
                    TbxObsVacunas.Enabled = True
                    BtnAgregarVacunas.Enabled = True
                    LbObsVacunas.Visible = True
                    TbxObsVacunas.Visible = True
                    BtnAgregarVacunas.Visible = True

                    TbxObsVacunas.Text = dtDatosVacunas.Rows(0).Item("obs").ToString

                End If

            Else

                lblInfoAntVacuna.Text = ""
