                TbxObsVacunas.Enabled = False
                BtnAgregarVacunas.Enabled = False

                LbObsVacunas.Visible = False
                TbxObsVacunas.Visible = False
                BtnAgregarVacunas.Visible = False

            End If

        Catch ex As Exception
