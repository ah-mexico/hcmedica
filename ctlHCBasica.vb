    Private Sub ManejoEsquemaVacunacion(ByVal EsqCompleto As Integer, ByVal Observacion As String)

        Try

            ErrorAntecedenteModulo = "ManejoEsquemaVacunacion"

            Dim objActualizarAP As New BLHistoriaBasica
            Dim Datos() As Object
            Dim lError As Long
            Dim dtRows As DataRow()
            Dim dtRow As DataRow

