    Private Sub dtgAntecedentesFarmacologicos_CellContentClick(ByVal sender As System.Object, ByVal e As System.Windows.Forms.DataGridViewCellEventArgs) Handles dtgAntecedentesFarmacologicos.CellContentClick

        Try
            If e.RowIndex = -1 Then
                Return
            End If

            If dtgAntecedentesFarmacologicos.Rows(e.RowIndex).Cells("EditableAfarma").Value = 0 Then

                If dtgAntecedentesFarmacologicos.Columns(e.ColumnIndex).Name = "ConfirmaAFarma" Then

                    Dim row As DataGridViewRow = dtgAntecedentesFarmacologicos.Rows(e.RowIndex)

                    Dim cellSelecion As DataGridViewCheckBoxCell = TryCast(row.Cells("NiegaAFarma"), DataGridViewCheckBoxCell)

                    If Convert.ToBoolean(cellSelecion.Value) Then
                        row.Cells("NiegaAFarma").Value = 0
                    End If

                Else
                    If dtgAntecedentesFarmacologicos.Columns(e.ColumnIndex).Name = "NiegaAFarma" Then

                        Dim row As DataGridViewRow = dtgAntecedentesFarmacologicos.Rows(e.RowIndex)

                        Dim cellSelecion As DataGridViewCheckBoxCell = TryCast(row.Cells("ConfirmaAFarma"), DataGridViewCheckBoxCell)

                        If Convert.ToBoolean(cellSelecion.Value) Then
                            row.Cells("ConfirmaAFarma").Value = 0
                        End If

                    Else
                        Return
                    End If
                End If

                dtgAntecedentesFarmacologicos.CommitEdit(DataGridViewDataErrorContexts.Commit)
                'Else
                'MessageBox.Show("No Es Permitido Modificar Este Registro")
            End If





        Catch ex As Exception

        End Try

    End Sub

    Private Sub dtgAntecedentesFarmacologicos_CellValueChanged(ByVal sender As System.Object, ByVal e As System.Windows.Forms.DataGridViewCellEventArgs) Handles dtgAntecedentesFarmacologicos.CellValueChanged

        Dim comodin As String
        If e.RowIndex >= 0 Then

            If String.IsNullOrEmpty(dtgAntecedentesFarmacologicos.CurrentRow.Cells(e.ColumnIndex).Value.ToString) Then
                comodin = 0
                dtgAntecedentesFarmacologicos.Rows(e.RowIndex).Cells(dtgAntecedentesFarmacologicos.CurrentRow.Cells(e.ColumnIndex).OwningColumn.Name).Value = 0

            Else
                comodin = dtgAntecedentesFarmacologicos.CurrentRow.Cells(e.ColumnIndex).Value.ToString
            End If

            If comodin <> System.DBNull.Value.ToString Then
                ModificarAntecedenteFarmacologico(dtgAntecedentesFarmacologicos.CurrentRow, sender, dtgAntecedentesFarmacologicos.CurrentRow.Cells(e.ColumnIndex).OwningColumn.HeaderText)
            End If
        End If

    End Sub


