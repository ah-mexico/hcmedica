    Private Sub cmdAdicionarAT_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles cmdAdicionarAT.Click

        If ValidarCamposObligatorios(pnlDatosAntecedentesToxicos) = False Then
            MessageBox.Show("Falta Información en Campo Obligatorio")
            Exit Sub
        End If

        'nueva validacion cdqf
        If validaopcionnulltoxicos() = False Then
            If txtCantTiempoConsumoAT.Text.ToString = "" Then
                MessageBox.Show("Falta Información en Campo Obligatorio - cantidad campo Durante ")
                Exit Sub
            End If
        End If

        If ValidoMesAnno(tbxvalmes.Text, tbxaniotoxico.Text) = 2 Then
            MessageBox.Show("Debe validar los datos registrados")
            tbxvalmes.Focus()
            Exit Sub
        End If
        'version ajuste sin errores
        If strDatoModificado.Trim.Length = 0 Then
            AdicionarNuevoAntecedenteToxico()
        Else
            ModificarAntecedenteToxico(dtgAntecedentesToxicos.CurrentRow, sender, String.Empty)
        End If
        strDatoModificado = ""
        ErrorAntecedenteModulo = ""
        'txtCodClaseAT.Enabled = True
        'txtCodClaseAT.Focus()

        txtCodClaseAT.Enabled = True
        txtDescClaseAT.Enabled = True
        tbxNombreToxico.Enabled = True
        txtDescClaseAT.Focus()

    End Sub
    Private Function validaopcionnulltoxicos() As Boolean
