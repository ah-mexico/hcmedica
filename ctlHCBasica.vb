                Datos(6) = objDatosPaciente.NumeroDocumento
                Datos(7) = drFila.Cells("descripcionAFar").Value.ToString
                Datos(8) = drFila.Cells("obsAFar").Value.ToString
                Datos(9) = lError

                lError = objActualizarAP.NEliminaAntecedenteNoVademecum(objConexion, lError, Datos)

            End If

            If lError <> 0 Then
                MessageBox.Show("Error al Eliminar Datos Antecedentes Farmacologicos")
            Else
                objDatosGenerales.HistoriaClinicaTieneModificaciones = True
                dtgAntecedentesFarmacologicos.Height = dtgAntecedentesFarmacologicos.ColumnHeadersHeight + (dtgAntecedentesFarmacologicos.Rows.GetRowsHeight(DataGridViewElementStates.None))
            End If

            ErrorAntecedenteModulo = ""

        Catch ex As Exception
            ControlAntecedentesHb(ex.Source & " - " & Environment.MachineName.ToString & " - " & ErrorAntecedenteModulo, ex.Message, ex.StackTrace & " - " & ex.TargetSite.ToString)
            MessageBox.Show("Error al Eliminar Datos Antecedentes Farmacologicos")
        End Try
    End Sub

