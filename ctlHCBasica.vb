    Private Sub AdicionarNuevoAntecedentePerinatal()
        Dim objActualizarAP As New BLHistoriaBasica
        Dim Datos() As Object
        Dim lError As Long

        ReDim Datos(17)
        Datos(0) = objDatosGenerales.Prestador          '@strCodPreSgs
        Datos(1) = objDatosGenerales.Sucursal           '@strCodSucur
        Datos(2) = objDatosPaciente.TipoDocumento       '@strTipDoc
        Datos(3) = objDatosPaciente.NumeroDocumento     '@strNumDoc
        Datos(4) = objDatosPaciente.TipoAdmision        '@strTipAdm
        Datos(5) = objDatosPaciente.AnoAdmision         '@intAnoAdm
        Datos(6) = objDatosPaciente.NumeroAdmision      '@dblNumAdm
        Datos(7) = Val(txtEdadGestacional.Text.ToString)        '@EdadGestacional
        Datos(8) = IIf(rbtUnica.Checked, 1, IIf(rbtMultiple.Checked, 2, DBNull.Value)) '@TipoGestacion
        Datos(9) = Val(txtPesoAPE.Text.ToString)       '@intPeso
        Datos(10) = Val(txtTallaAPE.Text.ToString)      '@intTalla
        Datos(11) = Val(txtPCefalicoAPE.Text.ToString)  '@intPCefalico
        Datos(12) = Val(txtPAbdominalAPE.Text.ToString) '@P_Abdominal
        Datos(13) = IIf(txtOtroComRN.Visible, txtOtroComRN.Text.ToString, String.Empty)  '@OtraComplicacionRN
        Datos(14) = IIf(txtOtroComParto.Visible, txtOtroComParto.Text.ToString, String.Empty)  '@OtraComplicacionParto
        Datos(15) = objDatosGenerales.Login             '@strLogin
        Datos(16) = objDatosGenerales.CodigoEspecialidad
        Datos(17) = lError                              '@lError

        Try

            ErrorAntecedenteModulo = "AdicionarNuevoAntecedentePerinatal"

            lError = objActualizarAP.ActualizarAntecedentePerinatal(objConexion, lError, Datos)
            If lError <> 0 Then
                MessageBox.Show("Error al Grabar Datos Antecedentes Perinatales")
            Else

                ReDim Datos(2)
                Datos(0) = objDatosPaciente.TipoDocumento       '@strTipDoc
                Datos(1) = objDatosPaciente.NumeroDocumento     '@strNumDoc
                Datos(2) = lError

                lError = objActualizarAP.EliminaAntecedentePerinatalComplicacion(objConexion, lError, Datos)
                lError = objActualizarAP.EliminaAntecedentePerinatalTipoParto(objConexion, lError, Datos)


                If chlComplicacionesRN.CheckedItems.Count <> 0 Then

                    ReDim Datos(6)
                    Datos(0) = objDatosGenerales.Prestador          '@strCodPreSgs
                    Datos(1) = objDatosGenerales.Sucursal           '@strCodSucur
                    Datos(2) = objDatosPaciente.TipoDocumento       '@strTipDoc
                    Datos(3) = objDatosPaciente.NumeroDocumento     '@strNumDoc
                    Datos(5) = objDatosGenerales.Login             '@strLogin
                    Datos(6) = lError
                    Dim x As Integer
                    Dim s As String = ""
                    For x = 0 To chlComplicacionesRN.CheckedItems.Count - 1
                        Datos(4) = CType(chlComplicacionesRN.CheckedItems(x), DataRowView)("IdComplicacion")
                        lError = objActualizarAP.ActualizarAntecedentePerinatalComplicacion(objConexion, lError, Datos)
                    Next x
                End If

                If chlComplicacionesParto.CheckedItems.Count <> 0 Then

                    ReDim Datos(6)
                    Datos(0) = objDatosGenerales.Prestador          '@strCodPreSgs
                    Datos(1) = objDatosGenerales.Sucursal           '@strCodSucur
                    Datos(2) = objDatosPaciente.TipoDocumento       '@strTipDoc
                    Datos(3) = objDatosPaciente.NumeroDocumento     '@strNumDoc
                    Datos(5) = objDatosGenerales.Login             '@strLogin
                    Datos(6) = lError
                    Dim x As Integer
                    Dim s As String = ""
                    For x = 0 To chlComplicacionesParto.CheckedItems.Count - 1
                        Datos(4) = CType(chlComplicacionesParto.CheckedItems(x), DataRowView)("IdComplicacion")
                        lError = objActualizarAP.ActualizarAntecedentePerinatalComplicacion(objConexion, lError, Datos)
                    Next x
                End If

                If chlTipoParto.CheckedItems.Count <> 0 Then

                    ReDim Datos(6)
                    Datos(0) = objDatosGenerales.Prestador          '@strCodPreSgs
                    Datos(1) = objDatosGenerales.Sucursal           '@strCodSucur
                    Datos(2) = objDatosPaciente.TipoDocumento       '@strTipDoc
                    Datos(3) = objDatosPaciente.NumeroDocumento     '@strNumDoc
                    Datos(5) = objDatosGenerales.Login             '@strLogin
                    Datos(6) = lError
                    Dim x As Integer
                    Dim s As String = ""
                    For x = 0 To chlTipoParto.CheckedItems.Count - 1
                        Datos(4) = CType(chlTipoParto.CheckedItems(x), DataRowView)("tip_Parto")
                        lError = objActualizarAP.ActualizarAntecedentePerinatalTipoParto(objConexion, lError, Datos)
                    Next x
                End If


                objDatosGenerales.HistoriaClinicaTieneModificaciones = True
                'LimpiarPantallaAntecedentesPerinatales()
            End If
        Catch ex As Exception
            ControlAntecedentesHb(ex.Source & " - " & Environment.MachineName.ToString & " - " & ErrorAntecedenteModulo, ex.Message, ex.StackTrace & " - " & ex.TargetSite.ToString)
            MessageBox.Show("Error al Grabar Datos Antecedentes Perinatales")
