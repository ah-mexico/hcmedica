    Private Sub ActualizarInformacionMotivoConsulta()
        'txtInfoSuministradaPor.Text = IIf(objHistoriaBasica.MotivoDeConsulta.InformacionSuministradaPor.Trim.Length > 0, objHistoriaBasica.MotivoDeConsulta.InformacionSuministradaPor, "PACIENTE")
        txtInfoSuministradaPor.Text = IIf(objHistoriaBasica.MotivoDeConsulta.InformacionSuministradaPor.Trim.Length > 0, objHistoriaBasica.MotivoDeConsulta.InformacionSuministradaPor, "")
        txtMotivoConsulta.Text = objHistoriaBasica.MotivoDeConsulta.MotivoDeConsulta
        txtEnfermedadActual.Text = objHistoriaBasica.MotivoDeConsulta.EnfermedadActual
        dtgRevisionSistemasMC.DataSource = objHistoriaBasica.MotivoDeConsulta.RevisionPorSistemasMotivoConsulta
        If dtDatosRiesgoNutricional.Rows.Count > 0 Then
            PuntajeNutricional = dtDatosRiesgoNutricional.Rows(0)("puntaje_riesgo")
            Select Case dtDatosRiesgoNutricional.Rows(0)("id_perdida_apetito")
                Case Is = 0
                    rbtNoPerdidaApetito.Checked = True
                Case Is = 1
                    rbtSiPerdidaApetito.Checked = True
            End Select

            Select Case dtDatosRiesgoNutricional.Rows(0)("id_perdida_peso")
                Case Is = 0
                    rbtNoPerdidaPeso.Checked = True
                Case Is = 1
                    rbtSiPerdidaPeso.Checked = True
                Case Is = 2
                    rbtNoSabePerdidaPeso.Checked = True
            End Select

            Select Case dtDatosRiesgoNutricional.Rows(0)("id_nivel_perdida_peso")
                Case Is = 1
                    rbtEntre1y6.Checked = True
                Case Is = 2
                    rbtEntre6y10.Checked = True
                Case Is = 3
                    rbtEntre10y15.Checked = True
                Case Is = 4
                    rbtMas15.Checked = True
                Case Is = 4
                    rbtNoSabe.Checked = True
            End Select

        Else
            rbtNoPerdidaApetito.Checked = False
            rbtSiPerdidaApetito.Checked = False

            rbtNoPerdidaPeso.Checked = False
            rbtSiPerdidaPeso.Checked = False
            rbtNoSabePerdidaPeso.Checked = False

            rbtEntre1y6.Checked = False
            rbtEntre6y10.Checked = False
            rbtEntre10y15.Checked = False
            rbtMas15.Checked = False
            rbtNoSabe.Checked = False
            lblPuntajeRiesgoNutricional.Text = String.Empty
        End If
    End Sub

    Private Sub ModificarMotivoDeConsulta()
        Dim objActualizarAP As New BLHistoriaBasica
        Dim Datos() As Object
        Dim lError As Long
        Dim intRow As Integer
        Dim blnModificaciones As Boolean
        Dim intDato As Integer


        blnModificaciones = False

        If txtInfoSuministradaPor.Text = txtInfoSuministradaPor.Tag And txtMotivoConsulta.Text = txtMotivoConsulta.Tag And
            txtEnfermedadActual.Text = txtEnfermedadActual.Tag Then
            blnModificaciones = False

        Else
            blnModificaciones = True
        End If

        If blnModificaciones = False Then
            'MsgBox("No hay Modificaciones Hechas Al Registro")
            Exit Sub
        End If

        ReDim Datos(12)
        Datos(0) = 1
        Datos(1) = objDatosPaciente.TipoDocumento
        Datos(2) = objDatosPaciente.NumeroDocumento
        'For intDato = 0 To 10 'dtgRevisionSistemasMC.RowCount - 1
        '    Datos(dtgRevisionSistemasMC.Rows(intDato).Cells("sistemaMC").Value + 2) = dtgRevisionSistemasMC.Rows(intDato).Cells("obsMC").Value.ToString
        'Next
        Datos(3) = objDatosGenerales.Prestador
        Datos(4) = objDatosGenerales.Sucursal
        Datos(5) = objDatosPaciente.TipoAdmision
        Datos(6) = objDatosPaciente.AnoAdmision
        Datos(7) = objDatosPaciente.NumeroAdmision
        Datos(8) = txtInfoSuministradaPor.Text.ToString
        Datos(9) = txtMotivoConsulta.Text.ToString
        Datos(10) = txtEnfermedadActual.Text.ToString
        Datos(11) = objDatosGenerales.Login
        Datos(12) = lError

        Try
            lError = objActualizarAP.ActualizarMotivoDeConsulta(objConexion, lError, Datos)
            If lError <> 0 Then
                MessageBox.Show("Error al Modificar Datos Motivo de Consulta")
            Else
                objDatosGenerales.HistoriaClinicaTieneModificaciones = True
                ActualizarInformacionObjetoMotivoConsulta(Datos(8), Datos(9), Datos(10))
                ActualizarInformacionControles()
            End If
        Catch ex As Exception
            ControlFinalizarHcb(ex.Source, ex.Message, ex.StackTrace)
            MessageBox.Show("Error al Grabar Datos Motivo de Consulta")
        End Try
    End Sub

    Private Sub ModificarRevisionSistemas()
        Dim objActualizarAP As New BLHistoriaBasica
        Dim Datos() As Object
        Dim lError As Long
        Dim intRow As Integer
        Dim blnModificaciones As Boolean
        Dim intDato As Integer


        Try
            ReDim Datos(11)

            Datos(0) = objDatosPaciente.TipoDocumento
            Datos(1) = objDatosPaciente.NumeroDocumento
            Datos(2) = objDatosGenerales.Prestador
            Datos(3) = objDatosGenerales.Sucursal
            Datos(4) = objDatosPaciente.TipoAdmision
            Datos(5) = objDatosPaciente.AnoAdmision
            Datos(6) = objDatosPaciente.NumeroAdmision
            Datos(10) = objDatosGenerales.Login
            Datos(11) = lError

            For intDato = 0 To dtgRevisionSistemasMC.RowCount - 1

                If dtgRevisionSistemasMC.Rows(intDato).Cells("Refiere").Value = True Or dtgRevisionSistemasMC.Rows(intDato).Cells("NoRefiere").Value = True Then
                    If dtgRevisionSistemasMC.Rows(intDato).Cells("Refiere").Value = True Then
                        Datos(7) = dtgRevisionSistemasMC.Rows(intDato).Cells("IdSistema").Value.ToString
                        Datos(8) = dtgRevisionSistemasMC.Rows(intDato).Cells("obsMC").Value.ToString
