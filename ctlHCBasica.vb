    Private Function ValidarCamposObligatorios(ByVal ctlContenedor As Panel) As Boolean
        Dim ctlControlTexto As TextBoxConFormato
        Dim ctlControlCombo As ComboBusqueda
        Dim intControl As Integer
        Dim blnRespuesta As Boolean

        blnRespuesta = True
        For intControl = 0 To ctlContenedor.Controls.Count - 1
            If TypeOf (ctlContenedor.Controls(intControl)) Is TextBoxConFormato Then
                ctlControlTexto = ctlContenedor.Controls(intControl)
                If ctlControlTexto.Obligatorio = True Then
                    If ctlControlTexto.Text.Trim.Length = 0 Then
                        ctlControlTexto.Focus()
                        blnRespuesta = False
                        Exit For
                    End If
                End If
            Else
                If TypeOf (ctlContenedor.Controls(intControl)) Is ComboBusqueda Then
                    ctlControlCombo = ctlContenedor.Controls(intControl)
                    If ctlControlCombo.Obligatorio = True Then
                        If ctlControlCombo.Text.Trim.Length = 0 Then
                            ctlControlCombo.Focus()
                            blnRespuesta = False
                            Exit For
                        End If
                    End If
                End If
            End If
        Next



        Return blnRespuesta
    End Function


    Private Function ValidoMesAnnoAntFam(ByVal strMes As String, ByVal strAnno As String) As Integer

        Dim intRespuesta As Integer

        Dim intAnno As Integer
        Dim intMes As Integer

        If strMes.Trim.Length = 0 And strAnno.Trim.Length = 0 Then
            Return 0 'Campos Vacios
        End If

        If strMes.Trim.Length > 0 And strAnno.Trim.Length = 0 Then
            Return 2 'Año y Mes no son Válidos
        End If

        If strMes.Trim.Length = 0 And strAnno.Trim.Length > 0 Then
            intAnno = Val(strAnno.Trim)
            If intAnno > Year(Now) Then
                Return 2
            Else
                Return 1 'Validación OK
            End If
        End If

        If strMes.Trim.Length > 0 And strAnno.Trim.Length > 0 Then
            intMes = Val(strMes.Trim)
            intAnno = Val(strAnno.Trim)
            intRespuesta = 1
            If intAnno > Year(Now) Then
                intRespuesta = 2
            Else

                If intAnno = Year(Now) Then
                    If intMes > Month(Now) Then
                        intRespuesta = 2
                    End If
                End If
            End If
            Return intRespuesta
        End If
    End Function
    Private Function ValidoMesAnno(ByVal strMes As String, ByVal strAnno As String) As Integer

        Dim intRespuesta As Integer

        Dim intAnno As Integer
        Dim intMes As Integer

        If strMes.Trim.Length = 0 And strAnno.Trim.Length = 0 Then
            Return 0 'Campos Vacios
        End If

        If strMes.Trim.Length > 0 And strAnno.Trim.Length = 0 Then
            Return 2 'Año y Mes no son Válidos
        End If

        If strMes.Trim.Length = 0 And strAnno.Trim.Length > 0 Then
            intAnno = Val(strAnno.Trim)
            If intAnno < Year(objDatosPaciente.FechaNacimiento) Or intAnno > Year(Now) Then
                Return 2
            Else
                Return 1 'Validación OK
            End If
        End If

        If strMes.Trim.Length > 0 And strAnno.Trim.Length > 0 Then
            intMes = Val(strMes.Trim)
            intAnno = Val(strAnno.Trim)
            intRespuesta = 1
            If intAnno < Year(objDatosPaciente.FechaNacimiento) Or intAnno > Year(Now) Then
                intRespuesta = 2
            Else
                If intAnno = Year(objDatosPaciente.FechaNacimiento) Then
                    If intMes < Month(objDatosPaciente.FechaNacimiento) Then
                        intRespuesta = 2
                    End If
                End If
                If intAnno = Year(Now) Then
                    If intMes > Month(Now) Then
                        intRespuesta = 2
                    End If
                End If
            End If
            Return intRespuesta
        End If
    End Function

