    Private Sub ActualizarInformacionImpresionDiagnostica()


        Try

            With objHistoriaBasica.ImpresionDiagnostica
                txtCodCausaExternaID.Text = .CodigoCausaExterna
                If txtCodCausaExternaID.Text.Trim.Length > 0 Then
                    txtDescCausaExternaID.Text = txtCodCausaExternaID.DescripcionCodigo
                End If
                txtCodCondicionUsuariaID.Text = .CodigoCondicionUsuaria
                If txtCodCondicionUsuariaID.Text.Trim.Length > 0 Then
                    txtDescCondicionUsuariaID.Text = txtCodCondicionUsuariaID.DescripcionCodigo
                End If

                Select Case .SospechaATEP.ToString
                    Case "N"
                        optSospechaATEPNO.Checked = True
                        optSospechaATEPSI.Checked = False
                    Case "S"
                        optSospechaATEPSI.Checked = True
                        optSospechaATEPNO.Checked = False
                End Select

                txtAmpliacionOcupacionID.Text = .AmpliacionDeLaOcupacion
                txtTiempoAntOcupacionID.Text = ReemplazarString(.TiempoDeAntiguedadEnLaOcupacion, 0, "")
                txtCodAntiguedadOcupacionID.Text = .CodigoAntiguedadEnLaOcupacion.ToString
                If txtCodAntiguedadOcupacionID.Text.Trim.Length > 0 Then
                    txtDescAntiguedadOcupacionID.Text = txtCodAntiguedadOcupacionID.DescripcionCodigo
                End If

                Select Case .SospechaRAM.ToString
                    Case "N"
                        optSospechaRAMNO.Checked = True
                        optSospechaRAMSI.Checked = False
                    Case "S"
                        optSospechaRAMSI.Checked = True
                        optSospechaRAMNO.Checked = False

                End Select

                txtObservacionesRAM.Text = .ReaccionAdversa.ToString
                txtPlanManejoID.Text = .DescripcionPlandeManejo
                TbxAnalisisGeneral.Text = .DescripcionAnalisis
                dtgImpresionDiagnostica.DataSource = objHistoriaBasica.ImpresionDiagnostica.DatosDiagnosticos

                If .ExplicaPlanManejo = "S" Then
                    rdPlaManPrg1Si.Checked = True
                ElseIf .ExplicaPlanManejo = "N" Then
                    rdPlaManPrg1No.Checked = True
                Else
                    rdPlaManPrg1Si.Checked = False
                    rdPlaManPrg1No.Checked = False
                End If

                'Me.AutoScrollPosition = New Point(0, 0)
                txtInfoSuministradaPor.Focus()
            End With
