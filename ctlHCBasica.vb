    Private Sub AdicionarNuevoAntecedenteFamiliar()
        Dim objActualizarAP As New BLHistoriaBasica
        Dim Datos() As Object
        Dim lError As Long
        Dim dtRows As DataRow()
        Dim dtRow As DataRow

        If dtgAntecedentesFamiliares.DataSource.Rows.Count > 0 Then
            dtRows = dtgAntecedentesFamiliares.DataSource.Select("diagnost='" & txtCodDiagnosticoAF.Text.ToString & "' AND parentesco='" & txtCodParentescoAF.Text.ToString & "'")

            If dtRows.Length > 0 Then
                MessageBox.Show("El Registro Ya Existe")
                Exit Sub
            End If

            dtRows = dtgAntecedentesFamiliares.DataSource.Select("diagnost='" & txtCodDiagnosticoAF.Text.ToString & "'")

            If dtRows.Length > 0 Then
                If MessageBox.Show("Este Diagnóstico Ya Existe en los Antecedentes. Desea Registrarlo Nuevamente?", "Antecedentes Familiares", MessageBoxButtons.YesNo) = DialogResult.No Then
                    Exit Sub
                End If
            End If
        End If

        ReDim Datos(21)
        Datos(0) = objDatosGenerales.Prestador
        Datos(1) = objDatosGenerales.Sucursal
        Datos(2) = objDatosPaciente.TipoDocumento
        Datos(3) = objDatosPaciente.NumeroDocumento
        Datos(4) = objDatosPaciente.TipoAdmision
        Datos(5) = objDatosPaciente.AnoAdmision
        Datos(6) = objDatosPaciente.NumeroAdmision
        Datos(7) = txtCodDiagnosticoAF.Text.ToString
        Datos(8) = txtCodParentescoAF.Text.ToString
        Datos(9) = objDatosGenerales.Login
        Datos(10) = cboDescDiagnosticoAF.OrigenDeDatos.Select("codigo='" & txtCodDiagnosticoAF.Text & "'")(0).Item("Clasificacion").ToString
        Datos(11) = 1 ' Confirma
        Datos(12) = 0 'Niega
