                    End If
                    .Item("ano_mamografia") = Datos(22)
                    .Item("mes_mamografia") = Datos(23)
                    If (chbNoRecuerdaMamografia.Checked) Then
                        .Item("Mamografia") = "No Recuerda"
                    ElseIf (chbNuncaMamografia.Checked) Then
                        .Item("Mamografia") = "Nunca"
                    Else
