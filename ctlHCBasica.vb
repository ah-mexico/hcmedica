    Private Sub ValidarTextosEgresoImpresionDiagnostica(ByVal sender As Object, ByVal e As System.ComponentModel.CancelEventArgs) Handles txtCodCondicionUsuariaID.Validating, txtDescCondicionUsuariaID.Validating, txtDescAntiguedadOcupacionID.Validating, txtTiempoAntOcupacionID.Validating

        Select Case sender.name.ToString
            Case "txtCodAntiguedadOcupacionID"
                cmdGuardarEID.Focus()
            Case "txtCodCausaExternaID", "txtDescCausaExternaID"
                Select Case txtCodCausaExternaID.Text
                    Case "01", "14"
                        optSospechaATEPSI.Checked = True
                        optSospechaATEPNO.Enabled = False
                        Me.txtAmpliacionOcupacionID.Enabled = True
                        Me.txtTiempoAntOcupacionID.Enabled = True
                        Me.txtDescAntiguedadOcupacionID.Enabled = True
                    Case Else
                        optSospechaATEPNO.Enabled = True
                        optSospechaATEPNO.Checked = True
                        Me.txtAmpliacionOcupacionID.Enabled = False
                        Me.txtTiempoAntOcupacionID.Enabled = False
                        Me.txtDescAntiguedadOcupacionID.Enabled = False
                End Select
            Case "txtDescAntiguedadOcupacionID"
                If txtDescAntiguedadOcupacionID.Text = "NINGUNO" Then
                    txtTiempoAntOcupacionID.ResetText()
                End If
            Case "txtTiempoAntOcupacionID"
                If txtDescAntiguedadOcupacionID.Text = "NINGUNO" Then
                    txtDescAntiguedadOcupacionID.ResetText()
                End If
        End Select

    End Sub

    Private Sub ValidarOpcionesSospechaATEP(ByVal sender As Object, ByVal e As System.EventArgs) Handles optSospechaATEPNO.Click, optSospechaATEPSI.Click

        Dim blnHabilitar As Boolean

        If sender.checked = True Then
            txtAmpliacionOcupacionID.ResetText()
            txtTiempoAntOcupacionID.ResetText()
            txtCodAntiguedadOcupacionID.ResetText()
            txtDescAntiguedadOcupacionID.ResetText()
        End If

        If sender.Name.ToLower = "optsospechaatepno" Then
            blnHabilitar = False
        Else
            blnHabilitar = True
        End If

        txtAmpliacionOcupacionID.Enabled = blnHabilitar
        txtTiempoAntOcupacionID.Enabled = blnHabilitar
        txtCodAntiguedadOcupacionID.Enabled = blnHabilitar
        txtDescAntiguedadOcupacionID.Enabled = blnHabilitar

        If blnHabilitar = True Then
            txtAmpliacionOcupacionID.Focus()
        Else
            cmdGuardarEID.Focus()
        End If

    End Sub

    Private Sub ValidarOpcionesSospechaRAM(ByVal sender As Object, ByVal e As System.EventArgs) Handles optSospechaRAMNO.Click, optSospechaRAMSI.Click

        Dim blnHabilitar As Boolean

        If optSospechaRAMSI.Checked = False Then
            txtObservacionesRAM.ResetText()
        End If

        If sender.Name.ToLower = "optsospecharamno" Then
            blnHabilitar = False
        Else
            blnHabilitar = True
        End If

        txtObservacionesRAM.Enabled = blnHabilitar

        If blnHabilitar Then
            txtObservacionesRAM.Focus()
        End If

    End Sub

    Private Function ValidarCamposVaciosEID() As Boolean
        Dim blnRespuesta As Boolean
        blnRespuesta = txtCodCausaExternaID.Text.Trim.Length = 0 And
                       txtCodCondicionUsuariaID.Text.Trim.Length = 0 And
                       txtAmpliacionOcupacionID.Text.Trim.Length = 0 And
                       txtCodAntiguedadOcupacionID.Text.Trim.Length = 0 And
                       optSospechaATEPSI.Checked = False And
                       optSospechaATEPNO.Checked = False And
                       TbxAnalisisGeneral.Text.Trim.Length = 0 And
                       txtPlanManejoID.Text.Trim.Length = 0 And
                       optSospechaRAMSI.Checked = False And
                       optSospechaRAMNO.Checked = False And
                       txtObservacionesRAM.Text.Trim.Length = 0 And
                       rdPlaManPrg1Si.Checked = False And
                       rdPlaManPrg1No.Checked = False And
                       rdPlaManPrg2Si.Checked = False And
                       rdPlaManPrg2No.Checked = False
        If blnRespuesta = True Then
            blnRespuesta = txtCodCausaExternaID.Tag.ToString.Trim.Length > 0 And
                           txtCodCondicionUsuariaID.Tag.ToString.Trim.Length > 0 And
                           txtAmpliacionOcupacionID.Tag.ToString.Trim.Length > 0 And
                           txtCodAntiguedadOcupacionID.Tag.ToString.Trim.Length > 0 And
                           optSospechaATEPSI.Tag = True And
                           optSospechaATEPNO.Tag = True And
                           txtPlanManejoID.Tag.Trim.Length = 0 And
                           optSospechaRAMSI.Tag = False And
                           optSospechaRAMNO.Tag = False And
                           txtObservacionesRAM.Tag.Trim.Length = 0 And
                           rdPlaManPrg1Si.Tag = False And
                           rdPlaManPrg1No.Tag = False And
                           rdPlaManPrg2Si.Tag = False And
                           rdPlaManPrg2No.Tag = False
        End If

        Return blnRespuesta

    End Function

