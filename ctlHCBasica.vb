    Public Sub habilitarModificacionMotivoConsulta(ByVal dtDatos As DataTable)

        With dtDatos.Rows(0)
            If objDatosPaciente.EstadoInicialHistoria = Paciente.PENDIENTE Or
                objDatosPaciente.EstadoInicialHistoria = Paciente.ENLISTAESPERA Then

                objHistoriaBasica.MotivoDeConsulta.HabilitarInformacionSuministradaPor = True
                objHistoriaBasica.MotivoDeConsulta.HabilitarMotivoDeConsulta = True
                objHistoriaBasica.MotivoDeConsulta.HabilitarEnfermedadActual = True
            Else
                If .Item("Inf_SumPor").ToString.Trim.Length > 0 Then
                    objHistoriaBasica.MotivoDeConsulta.HabilitarInformacionSuministradaPor = False
                End If
                If .Item("mot_consulta").ToString.Trim.Length > 0 Then
                    objHistoriaBasica.MotivoDeConsulta.HabilitarMotivoDeConsulta = False
                End If
                If .Item("Enf_Actual").ToString.Trim.Length > 0 Then
                    objHistoriaBasica.MotivoDeConsulta.HabilitarEnfermedadActual = False
                End If
            End If
        End With
