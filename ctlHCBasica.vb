    Private Sub habilitarModificacionAntecedentes(ByRef dtdatos As DataTable)
        Dim cont As Integer

        If dtdatos Is Nothing Then
            Exit Sub
        End If

        For cont = 0 To dtdatos.Rows.Count - 1
            With dtdatos.Rows(cont)
                If .Item("tip_admision").ToString = objDatosPaciente.TipoAdmision And
                   .Item("ano_adm").ToString = objDatosPaciente.AnoAdmision And
                   .Item("num_adm").ToString = objDatosPaciente.NumeroAdmision And
                   (objDatosPaciente.EstadoInicialHistoria = Paciente.PENDIENTE Or
                    objDatosPaciente.EstadoInicialHistoria = Paciente.ENLISTAESPERA) Then

                    .Item("EstadoUsu") = "N"

                End If
            End With
        Next cont
    End Sub

