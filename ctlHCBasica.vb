        'dtgAntTraumatologicos.DataSource.AcceptChanges()
        'objHistoriaBasica.AntecedentesTrauma.DatosAntecedentes.AcceptChanges()

    End Sub

    Private Sub dtgAntTraumatologicos_UserDeletingRow(ByVal sender As System.Object, ByVal e As System.Windows.Forms.DataGridViewRowCancelEventArgs) Handles dtgAntTraumatologicos.UserDeletingRow

        If e.Row.Cells("EditableAtrauma").Value = "1" Then
            If MessageBox.Show("Esta Seguro de Borrar este Registro?", "Antecedentes Traumatologicos", MessageBoxButtons.YesNo, MessageBoxIcon.Question) = DialogResult.Yes Then
                EliminarAntecedenteTraumatologicos(e.Row)
            Else
                e.Cancel = True
            End If
        Else
            MessageBox.Show("No Es Permitido Eliminar Este Registro")
            e.Cancel = True
        End If

        'TraerDatosAntecedentesTraumatologicos()
        'MostrarInformacionATrauma()

        'If dtgAntTraumatologicos.RowCount > 0 Then
        '    CbxNoRefAtrauma.Enabled = False
        'Else
        '    CbxNoRefAtrauma.Enabled = True
        'End If

        TraerDatosAntecedentesTraumatologicos()


        If dtDatosATrauma.Rows.Count > 0 Then
            CbxNoRefAtrauma.Enabled = False
        Else
            CbxNoRefAtrauma.Enabled = True
        End If

        LimpiarPantallaAntTraumatologicos()
        TbxIdTipoTrauma.Focus()
    End Sub

