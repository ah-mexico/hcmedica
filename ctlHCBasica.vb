                    If dsDatosAPE.Tables(3).Rows.Count > 0 Then
                        For Each dr As DataRow In dsDatosAPE.Tables(3).Rows

                            For i As Integer = 0 To chlTipoParto.Items.Count - 1
                                RV = chlTipoParto.Items.Item(i)

                                If dr.Item("IdTipoParto") = RV.Row.Item("tip_Parto") Then
                                    chlTipoParto.SetItemChecked(i, True)
                                End If

                            Next
                        Next
                    End If

                    objHistoriaBasica.AntecedentesPerinatales.TieneInformacion = True
                    objHistoriaBasica.AntecedentesPerinatales.DatosAntecedentes = dsDatosAPE.Tables(0)
                    ActualizarInformacionAntecedentesPerinatales()
