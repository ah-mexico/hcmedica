    Public Sub permiteModificarDiagnosticos(ByRef dtDatos As DataTable)
        Dim cont As Integer
        Dim bModifica As Boolean

        If dtDatos Is Nothing Then
            Exit Sub
        End If

        bModifica = puedeModificarImpresionDiagn()

        For cont = 0 To dtDatos.Rows.Count - 1
            If bModifica Then
                dtDatos.Rows(cont).Item("EstadoUsu") = "N"
            End If
        Next cont

    End Sub

