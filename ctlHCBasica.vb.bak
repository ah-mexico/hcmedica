    Private Sub bgwCargarInfoInicial2_RunWorkerCompleted(ByVal sender As Object, ByVal e As System.ComponentModel.RunWorkerCompletedEventArgs) Handles bgwCargarInfoInicial2.RunWorkerCompleted
        Try
            If blnFallaConsultaDatosAA = False Then
                MostrarInformacionAA()
            End If
