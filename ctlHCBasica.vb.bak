    Private Sub AdicionarNuevoImpresionDiagnostica()
        Dim objActualizarAP As New BLHistoriaBasica
        Dim Datos() As Object
        Dim lError As Long
        Dim dtRows As DataRow()
        Dim dtRow As DataRow
        Dim bAntecedente As Boolean


        If dtgImpresionDiagnostica.DataSource.Rows.Count > 0 Then

            ''Validar que no se pueda adicionar mas de un diagnostico principal
            If txtCodCategoriaID.Text = "P" And objHistoriaBasica.ImpresionDiagnostica.TieneDiagnosticoPrincipal = True Then
                MessageBox.Show("Ya Existe un Diagnóstico Principal")
                Exit Sub
            End If

            dtRows = dtgImpresionDiagnostica.DataSource.Select("cod_pre_sgs='" & objDatosGenerales.Prestador & "' AND cod_sucur='" & objDatosGenerales.Sucursal & _
                    "' AND tip_admision='" & objDatosPaciente.TipoAdmision & "' AND ano_adm=" & objDatosPaciente.AnoAdmision & " AND num_adm=" & objDatosPaciente.NumeroAdmision & _
                    " AND diagnost='" & txtCodDiagnosticoID.Text.ToString & "' AND tip_diag='" & txtCodTipoDiagnosticoID.Text.ToString & _
                    "' AND clase_diag='I'")

            If dtRows.Length > 0 Then
                MessageBox.Show("El Registro Ya Existe")
                Exit Sub
            End If

            If chkDiagnosticoAntecedenteID.Checked = True Then
                'dtRows = dtgImpresionDiagnostica.DataSource.Select("diagnost='" & txtCodDiagnosticoID.Text.ToString & "'")
                dtRows = dtgAntecedentesPatologicos.DataSource.Select("diagnost='" & txtCodDiagnosticoID.Text.ToString & "'")

                If dtRows.Length > 0 Then
                    If MessageBox.Show("Este Diagnóstico Ya Existe en los Antecedentes. Desea Registrarlo Nuevamente?", "Impresión Diagnóstica", MessageBoxButtons.YesNo) = DialogResult.No Then
                        bAntecedente = False
                        Exit Sub
                    Else
                        bAntecedente = True
                    End If
                End If
            End If

