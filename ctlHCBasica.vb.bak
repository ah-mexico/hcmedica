#Region "Procesos Riesgo Nutricional"

    Private Sub rbtSiPerdidaApetito_CheckedChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles rbtSiPerdidaApetito.CheckedChanged
        Try
            lblPuntajeRiesgoNutricional.Text = "Puntaje Riesgo Nutricional = "
            If rbtSiPerdidaApetito.Checked Then
                pnlRangoRespuestas.Visible = True
                PuntajeNutricional = 1
                lblPuntajeRiesgoNutricional.Text += PuntajeNutricional.ToString()
            Else
                pnlRangoRespuestas.Visible = False
                PuntajeNutricional = 0
                rbtEntre1y6.Checked = False
                rbtEntre6y10.Checked = False
                rbtEntre10y15.Checked = False
                rbtMas15.Checked = False
                rbtNoSabe.Checked = False
            End If
        Catch ex As Exception

        End Try
    End Sub

    Private Sub rbtEntre1y6_CheckedChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles rbtEntre1y6.CheckedChanged
        Try
            If rbtEntre1y6.Checked Then
                CalculaRiesgoNutricional()
            End If

        Catch ex As Exception

        End Try
    End Sub

    Private Sub rbtEntre6y10_CheckedChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles rbtEntre6y10.CheckedChanged
        Try
            If rbtEntre6y10.Checked Then
                CalculaRiesgoNutricional()
            End If

        Catch ex As Exception

        End Try
    End Sub

    Private Sub rbtEntre10y15_CheckedChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles rbtEntre10y15.CheckedChanged
        Try
            If rbtEntre10y15.Checked Then
                CalculaRiesgoNutricional()
            End If

        Catch ex As Exception

        End Try
    End Sub

    Private Sub rbtMas15_CheckedChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles rbtMas15.CheckedChanged
        Try
            If rbtMas15.Checked Then
                CalculaRiesgoNutricional()
            End If

        Catch ex As Exception

        End Try
    End Sub

    Private Sub rbtNoSabe_CheckedChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles rbtNoSabe.CheckedChanged
        Try
            If rbtNoSabe.Checked Then
                CalculaRiesgoNutricional()
            End If

        Catch ex As Exception

        End Try
    End Sub

    Private Sub CalculaRiesgoNutricional()
        Try
            PuntajeNutricional = 1
            lblPuntajeRiesgoNutricional.Text = "Puntaje Riesgo Nutricional = "

            If rbtEntre1y6.Checked Then
                PuntajeNutricional += 1
            End If
            If rbtEntre6y10.Checked Then
                PuntajeNutricional += 2
            End If
            If rbtEntre10y15.Checked Then
                PuntajeNutricional += 3
            End If
            If rbtMas15.Checked Then
                PuntajeNutricional += 4
            End If
            If rbtNoSabe.Checked Then
                PuntajeNutricional += 2
            End If

            lblPuntajeRiesgoNutricional.Text += PuntajeNutricional.ToString()
        Catch ex As Exception

        End Try
    End Sub

