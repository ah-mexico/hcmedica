    Private Sub cmdAdicionarID_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles cmdAdicionarID.Click
        Try
            If ValidarCamposObligatorios(pnlDatosImpresionDiagnostica) = False Then
                MessageBox.Show("Falta Información en Campo Obligatorio")
                Exit Sub
            End If

            If strDatoModificado.Trim.Length = 0 Then
                AdicionarNuevoImpresionDiagnostica()
            Else
                ModificarImpresionDiagnostica(dtgImpresionDiagnostica.CurrentRow)
            End If
            strDatoModificado = ""
            txtCodTipoDiagnosticoID.Enabled = True
            txtCodTipoDiagnosticoID.Focus()


        Catch ex As Exception
            EscribirError("Error: " & Err.Number.ToString & " " & Err.Description & vbCrLf & _
                          "Mensaje: " & ex.Message.ToString & vbCrLf & _
                          "Origen: " & ex.Source.ToString & vbCrLf, _
                          "Traza: " & ex.StackTrace & " - " & ex.TargetSite.ToString, objDatosGenerales.Login.ToString, Environment.MachineName.ToString)
            MsgBox("Error al Agregar Diagnóstico en Impresión Diagnóstica", MsgBoxStyle.Critical + MsgBoxStyle.OkOnly)

        End Try
    End Sub

