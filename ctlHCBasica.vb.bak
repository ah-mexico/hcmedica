                    .Cells("desResGestacionAPE").Value = txtDGestacionAP.Text
                End With

                dtgAntecedentesPerinatales.Height = dtgAntecedentesPerinatales.ColumnHeadersHeight + (dtgAntecedentesPerinatales.Rows.GetRowsHeight(DataGridViewElementStates.None))
                LimpiarPantallaAntecedentesPerinatales()
            End If
        Catch ex As Exception
            MessageBox.Show("Error al Grabar Datos Antecedentes Perinatales")
        End Try
    End Sub               'OK

    Private Sub EliminarAntecedentePerinatal(ByVal drFila As DataGridViewRow)
        Dim objActualizarAP As New BLHistoriaBasica
        Dim Datos() As Object
        Dim lError As Long

        ReDim Datos(24)
        Datos(0) = 2                                            '@intAccion
        Datos(1) = objDatosPaciente.TipoDocumento               '@strTipDoc
        Datos(2) = objDatosPaciente.NumeroDocumento             '@strNumDoc
        Datos(3) = ""                                           '@strDocumentosUnificados
        Datos(4) = objDatosGenerales.Prestador                  '@strCodPreSgs
        Datos(5) = objDatosGenerales.Sucursal                   '@strCodSucur
        Datos(6) = objDatosPaciente.TipoAdmision                '@strTipAdm
        Datos(7) = objDatosPaciente.AnoAdmision                 '@intAnoAdm
        Datos(8) = objDatosPaciente.NumeroAdmision              '@dblNumAdm
        Datos(9) = 0                                            '@intPeso
        Datos(10) = 0                                           '@intTalla
        Datos(11) = 0                                           '@intPCefalico
        Datos(12) = 0                                           '@strTipParto
        Datos(13) = ""                                          '@strResGestacion
        Datos(14) = ""                                          '@strHialina
        Datos(15) = ""                                          '@strOxigenoterapia
        Datos(16) = ""                                          '@strHipoglucemia
        Datos(17) = ""                                          '@strIncompatibleGrupo
        Datos(18) = ""                                          '@strIncompatibleFactor
        Datos(19) = ""                                          '@strTaquipnea
        Datos(20) = ""                                          '@Ventilacion
        Datos(21) = ""                                          '@strObs
        Datos(22) = "N"                                         '@strConfidencial
        Datos(23) = objDatosGenerales.Login                     '@strLogin
        Datos(24) = lError                                      '@lError

        Try
            lError = objActualizarAP.ActualizarAntecedentePerinatal(objConexion, lError, Datos)
            If lError <> 0 Then
                MessageBox.Show("Error al Eliminar Datos Antecedentes Perinatales")
            Else
                objDatosGenerales.HistoriaClinicaTieneModificaciones = True
                dtgAntecedentesPerinatales.Height = dtgAntecedentesPerinatales.ColumnHeadersHeight + (dtgAntecedentesPerinatales.Rows.GetRowsHeight(DataGridViewElementStates.None))
            End If
        Catch ex As Exception
            MessageBox.Show("Error al Eliminar Datos Antecedentes Perinatales")
        End Try
    End Sub                'OK

    Private Sub LimpiarPantallaAntecedentesPerinatales()
        txtPesoAPE.ResetText()
        txtTallaAPE.ResetText()
        txtPCefalicoAPE.ResetText()
        txtTipoPartoAP.ResetText()
        txtDTipoPartoAP.ResetText()
        txtGestacionAP.ResetText()
        txtDGestacionAP.ResetText()
        chkHialinaAPE.Checked = False
        chkOxigenoTerapiaAPE.Checked = False
        chkHipoglicemiaAPE.Checked = False
        chkIncGrupoAPE.Checked = False
        chkIncFactorAPE.Checked = False
        chkTaquipneaAPE.Checked = False
        chkVentilacionAPE.Checked = False
        chkConfidencialAPE.Checked = False
        txtObsAP.ResetText()
        strDatoModificado = ""
    End Sub      'OK


    Private Function ValidarCamposVaciosAPE() As Boolean


        Dim blnRespuesta As Boolean

        blnRespuesta = txtPesoAPE.Text.Trim.Length = 0 And _
                       txtTallaAPE.Text.Trim.Length = 0 And _
                       txtPCefalicoAPE.Text.Trim.Length = 0 And _
                       txtTipoPartoAP.Text.Trim.Length = 0 And _
                       txtGestacionAP.Text.Trim.Length = 0 And _
                       chkHialinaAPE.Checked = False And _
                       chkOxigenoTerapiaAPE.Checked = False And _
                       chkHipoglicemiaAPE.Checked = False And _
                       chkIncGrupoAPE.Checked = False And _
                       chkIncFactorAPE.Checked = False And _
                       chkVentilacionAPE.Checked = False And _
                       chkTaquipneaAPE.Checked = False

        If blnRespuesta = True Then
            Return True
        Else
            Return False
        End If

    End Function

    Private Sub cmdAdicionarAPE_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles cmdAdicionarAPE.Click

        If ValidarCamposVaciosAPE() Then
            MessageBox.Show("Falta Digitar Información. ")
            txtPesoAPE.Focus()
            Exit Sub
        End If


        If ValidarCamposObligatorios(pnlDatosAntecedentesPerinatales) = False Then
            MessageBox.Show("Falta Información en Campo Obligatorio")
            Exit Sub
        End If

        If strDatoModificado.Trim.Length = 0 Then

            AdicionarNuevoAntecedentePerinatal()
        Else
            ModificarAntecedentePerinatal(dtgAntecedentesPerinatales.CurrentRow)
        End If
        strDatoModificado = ""
    End Sub                       'OK

    Private Sub dtgAntecedentesPerinatales_Click(ByVal sender As Object, ByVal e As System.EventArgs) Handles dtgAntecedentesPerinatales.Click
        If dtgAntecedentesPerinatales.DataSource Is Nothing Then
            Exit Sub
        Else
            If dtgAntecedentesPerinatales.DataSource.Rows.Count <= 0 Then
                Exit Sub
            End If
        End If
        EditarDatosAntecedentesPerinatales(sender.CurrentRow)

    End Sub

    Private Sub dtgAntecedentesPerinatales_DataSourceChanged(ByVal sender As Object, ByVal e As System.EventArgs) Handles dtgAntecedentesPerinatales.DataSourceChanged
        dtgAntecedentesPerinatales.Height = dtgAntecedentesPerinatales.ColumnHeadersHeight + (dtgAntecedentesPerinatales.Rows.GetRowsHeight(DataGridViewElementStates.None))
    End Sub            'OK

    Private Sub dtgAntecedentesPerinatales_DoubleClick(ByVal sender As Object, ByVal e As System.EventArgs) Handles dtgAntecedentesPerinatales.DoubleClick
        If dtgAntecedentesPerinatales.DataSource Is Nothing Then
            Exit Sub
        Else
            If dtgAntecedentesPerinatales.DataSource.Rows.Count <= 0 Then
                Exit Sub
            End If
        End If

        EditarDatosAntecedentesPerinatales(sender.CurrentRow)
        If NVL(dtgAntecedentesPerinatales.CurrentRow.Cells("EstadoUsuAPE").Value) <> "N" Then
            MessageBox.Show("No Es Permitido Modificar Este Registro")
            Exit Sub
        End If
        strDatoModificado = "APE"

    End Sub

    Private Sub dtgAntecedentesPerinatales_UserDeletedRow(ByVal sender As Object, ByVal e As System.Windows.Forms.DataGridViewRowEventArgs) Handles dtgAntecedentesPerinatales.UserDeletedRow
        dtgAntecedentesPerinatales.DataSource.AcceptChanges()
        objHistoriaBasica.AntecedentesPerinatales.DatosAntecedentes.AcceptChanges()
    End Sub
    'OK

    Private Sub dtgAntecedentesPerinatales_UserDeletingRow(ByVal sender As Object, ByVal e As System.Windows.Forms.DataGridViewRowCancelEventArgs) Handles dtgAntecedentesPerinatales.UserDeletingRow

        If NVL(e.Row.Cells("EstadoUsuAPE").Value) = "N" Then
            If MessageBox.Show("Esta Seguro de Borrar este Registro?", "Antecedentes Ginecologicos", MessageBoxButtons.YesNo, MessageBoxIcon.Question) = DialogResult.Yes Then
                EliminarAntecedentePerinatal(e.Row)
            Else
                e.Cancel = True
            End If
        Else
            MessageBox.Show("No Es Permitido Eliminar Este Registro")
            e.Cancel = True
        End If

        LimpiarPantallaAntecedentesPerinatales()

    End Sub  'OK

    Private Sub Enter_Checks(ByVal sender As Object, ByVal e As System.Windows.Forms.KeyPressEventArgs) Handles chkHialinaAPE.KeyPress, chkOxigenoTerapiaAPE.KeyPress, chkHipoglicemiaAPE.KeyPress, chkIncGrupoAPE.KeyPress, chkIncFactorAPE.KeyPress, chkTaquipneaAPE.KeyPress, chkVentilacionAPE.KeyPress, chkConfidencialAPE.KeyPress
        If Asc(e.KeyChar) = 13 Then
            sender.Parent.GetNextControl(sender, True).Focus()
        End If
    End Sub

    Private Sub txtTipoPartoAP_Validated(ByVal sender As Object, ByVal e As System.EventArgs) Handles txtTipoPartoAP.Validated
        If Len(txtTipoPartoAP.Text) = 0 Then
            txtDTipoPartoAP.Text = ""
        End If
    End Sub

    Private Sub txtGestacionAP_Validated(ByVal sender As Object, ByVal e As System.EventArgs) Handles txtGestacionAP.Validated
        If Len(txtGestacionAP.Text) = 0 Then
            txtDGestacionAP.Text = ""
        End If
    End Sub


