    Private Sub AdicionarNuevoAntecedenteToxico()
        Dim objActualizarAP As New BLHistoriaBasica
        Dim Datos() As Object
        Dim lError As Long
        Dim dtRows As DataRow()
        Dim dtRow As DataRow

        If dtgAntecedentesToxicos.DataSource.Rows.Count > 0 Then
            dtRows = dtgAntecedentesToxicos.DataSource.Select("cod_pre_sgs='" & objDatosGenerales.Prestador & "' AND cod_sucur='" & objDatosGenerales.Sucursal & _
                    "' AND tip_admision='" & objDatosPaciente.TipoAdmision & "' AND ano_adm=" & objDatosPaciente.AnoAdmision & " AND num_adm=" & objDatosPaciente.NumeroAdmision & _
                    " AND toxico='0' AND clase='" & txtCodClaseAT.Text.ToString & "'")

            If dtRows.Length > 0 Then
                MessageBox.Show("El Registro Ya Existe")
                Exit Sub
            End If

            dtRows = dtgAntecedentesToxicos.DataSource.Select("toxico='0' AND clase='" & txtCodClaseAT.Text.ToString & "'")

            If dtRows.Length > 0 Then
                If MessageBox.Show("Esta Clase de Tóxico Ya Existe en los Antecedentes. Desea Registrarlo Nuevamente?", "Antecedentes Tóxicos", MessageBoxButtons.YesNo) = DialogResult.No Then
                    Exit Sub
                End If
            End If
        End If

        ReDim Datos(22)
        Datos(0) = 0
