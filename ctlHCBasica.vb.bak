    Private Sub pnlMotivoDeConsulta_Validating(ByVal sender As Object, ByVal e As System.ComponentModel.CancelEventArgs) Handles pnlMotivoDeConsulta.Validating

        Dim intRow As Integer
        Dim blnModificaciones As Boolean

        blnModificaciones = False
        If txtInfoSuministradaPor.Text = txtInfoSuministradaPor.Tag And txtMotivoConsulta.Text = txtMotivoConsulta.Tag And _
            txtEnfermedadActual.Text = txtEnfermedadActual.Tag Then
            For intRow = 0 To dtgRevisionSistemasMC.Rows.Count - 1
                If NVL(dtgRevisionSistemasMC.Rows(intRow).Cells("obsMC").Value) <> NVL(dtgRevisionSistemasMC.Rows(intRow).Cells("copiaobsMC").Value) Then
                    blnModificaciones = True
                    Exit For
                End If
                If NVL(dtgRevisionSistemasMC.Rows(intRow).Cells("Refiere").Value) <> NVL(dtgRevisionSistemasMC.Rows(intRow).Cells("copiaRefiere").Value) Then
                    blnModificaciones = True
                    Exit For
                End If

            Next
        Else
            blnModificaciones = True
        End If

        If blnModificaciones = False Then
            Exit Sub
        End If

        cmdGuardarMC_Click(sender, e)

    End Sub

    Public Sub habilitarModificacionMotivoConsulta(ByVal dtDatos As DataTable)

        With dtDatos.Rows(0)
            If objDatosPaciente.EstadoInicialHistoria = Paciente.PENDIENTE Or _
                objDatosPaciente.EstadoInicialHistoria = Paciente.ENLISTAESPERA Then

                objHistoriaBasica.MotivoDeConsulta.HabilitarInformacionSuministradaPor = True
                objHistoriaBasica.MotivoDeConsulta.HabilitarMotivoDeConsulta = True
                objHistoriaBasica.MotivoDeConsulta.HabilitarEnfermedadActual = True
            Else
                If .Item("Inf_SumPor").ToString.Trim.Length > 0 Then
                    objHistoriaBasica.MotivoDeConsulta.HabilitarInformacionSuministradaPor = False
                End If
                If .Item("mot_consulta").ToString.Trim.Length > 0 Then
                    objHistoriaBasica.MotivoDeConsulta.HabilitarMotivoDeConsulta = False
                End If
                If .Item("Enf_Actual").ToString.Trim.Length > 0 Then
                    objHistoriaBasica.MotivoDeConsulta.HabilitarEnfermedadActual = False
                End If
            End If
        End With
    End Sub

