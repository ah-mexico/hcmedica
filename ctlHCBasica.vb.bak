    Private Function ConsultarAdmisionesConEF_Errado(ByRef intHistorias As Integer) As Boolean
        Dim objDAOGeneral As New Sophia.HistoriaClinica.DAO.DAOGeneral
        Dim dtDatos As DataTable
        Dim strCondicion As String
        Dim blnRespuesta As Boolean

        strCondicion = "tip_doc='" & objDatosPaciente.TipoDocumento & "' AND " & _
                       "num_doc='" & objDatosPaciente.NumeroDocumento & "' AND login='" & objDatosGenerales.Login & "' AND estado='E'"

        dtDatos = objDAOGeneral.EjecutarSQLConParametros("hcCorreccion (NOLOCK)", objConexion, "cod_pre_sgs, cod_sucur, tip_admision, ano_adm, num_adm, tip_doc, num_doc, login_egreso, estado, fec_hc, fec_con, '" & objDatosGenerales.Login & "' as login, obs, 0 as Orden, login as login_original", strCondicion)


        intHistorias = dtDatos.Rows.Count

        blnRespuesta = intHistorias > 0

        If blnRespuesta = True Then
            objDatosPaciente.HistoriasConErroresEF = dtDatos
        End If

        Return blnRespuesta

    End Function
