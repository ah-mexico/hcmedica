    Private Sub EliminarRegistroDeVacuna(ByVal drFila As DataGridViewRow)
        Dim dtTablaVacunas As DataTable
        Dim intDosis As Integer
        Dim dtRowVacuna As DataRow
        Dim strXMLVacunas As String
        Dim objActualizarAP As New BLHistoriaBasica
        Dim Datos() As Object
        Dim lError As Long

        dtTablaVacunas = cboVacunas.DataSource
        dtRowVacuna = dtTablaVacunas.NewRow
        dtRowVacuna.Item("vacuna") = drFila.Cells("vacunav").Value.ToString
        dtRowVacuna.Item("descripcion") = drFila.Cells("descripcionv").Value.ToString
        cboVacunas.DataSource.Rows.Add(dtRowVacuna)
        cboVacunas.ResetText()

        dtTablaVacunas = CrearDatosVacunasActualizar()
        For intDosis = 1 To 6
            If drFila.Cells("seleccion" & intDosis).Value = True Then
                dtRowVacuna = dtTablaVacunas.NewRow()
                dtRowVacuna("vacuna") = drFila.Cells("vacunav").Value.ToString
                dtRowVacuna("dosis") = intDosis
                dtRowVacuna("ano_dosis") = drFila.Cells("anno" & intDosis.ToString).Value
                dtRowVacuna("mes_dosis") = drFila.Cells("mes" & intDosis.ToString).Value
                dtRowVacuna("accion") = 2
                dtTablaVacunas.Rows.Add(dtRowVacuna)
            End If
        Next

        If dtTablaVacunas.Rows.Count <= 0 Then
            Exit Sub
        End If

        strXMLVacunas = GenerarXMLxProcedimiento(dtTablaVacunas)

        ReDim Datos(13)
        Datos(0) = 1
        Datos(1) = objDatosPaciente.TipoDocumento
        Datos(2) = objDatosPaciente.NumeroDocumento
        Datos(3) = ""
        Datos(4) = strXMLVacunas
        Datos(5) = objDatosGenerales.Prestador
        Datos(6) = objDatosGenerales.Sucursal
        Datos(7) = objDatosPaciente.TipoAdmision
        Datos(8) = objDatosPaciente.AnoAdmision
        Datos(9) = objDatosPaciente.NumeroAdmision
        Datos(10) = txtObservacionesAFar.Text.ToString
        Datos(11) = "N"
        Datos(12) = objDatosGenerales.Login
        Datos(13) = lError

        Try
            lError = objActualizarAP.ActualizarVacunas(objConexion, lError, Datos)
            If lError <> 0 Then
                MessageBox.Show("Error al Eliminar Registro Vacunas")
            Else
                objDatosGenerales.HistoriaClinicaTieneModificaciones = True
                dtgVacunas.Height = dtgVacunas.ColumnHeadersHeight + (dtgVacunas.Rows.GetRowsHeight(DataGridViewElementStates.None))
            End If
        Catch ex As Exception
            MessageBox.Show("Error al Eliminar Registro Vacunas")
        End Try

    End Sub

    Private Sub AutoCompleteKeyUp(ByVal Combo As ComboBox, ByVal e As KeyEventArgs)
        Dim strTyped As String
        Dim intFoundIndex As Integer
        Dim objFoundItem As Object
        Dim strFoundText As String
        Dim strAppendText As String


        Select Case e.KeyCode
            Case Keys.Back, Keys.Left, Keys.Right, Keys.Up, Keys.Delete, Keys.Down, Keys.Home, Keys.ShiftKey, Keys.Shift, Keys.End, Keys.Tab
                Return
        End Select

        strTyped = Combo.Text
        intFoundIndex = Combo.FindString(strTyped)

        If intFoundIndex >= 0 Then
            objFoundItem = Combo.Items(intFoundIndex)
            strFoundText = Combo.GetItemText(objFoundItem)
            strAppendText = strFoundText.Substring(strTyped.Length)
            Combo.Text = strTyped & strAppendText
            Combo.Text = strTyped & strAppendText
            Combo.SelectionStart = strTyped.Length
            Combo.SelectionLength = strAppendText.Length
        End If
    End Sub

    Private Sub AutoCompleteLeave(ByVal Combo As ComboBox)
        Dim intFoundIndex As Integer
        intFoundIndex = Combo.FindStringExact(Combo.Text)
        Combo.SelectedIndex = -1
        Combo.SelectedIndex = intFoundIndex
    End Sub

