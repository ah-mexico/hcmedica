        dtgVacunas.Height = dtgVacunas.ColumnHeadersHeight + (dtgVacunas.Rows.GetRowsHeight(DataGridViewElementStates.None))
    End Sub

    Private Sub dtgVacunas_CellContentClick(ByVal sender As Object, ByVal e As System.Windows.Forms.DataGridViewCellEventArgs) Handles dtgVacunas.CellContentClick
        Dim intDosis As Integer
        Dim dtTable As DataTable
        Dim intCol As Integer
        Dim blnConObs As Boolean = True

        '0=Adicion, 1=Modificacion, 2=Eliminación
        '3=Viene de Consulta
        '6=Viene Null de Consulta
        If e.RowIndex < 0 Then
            Exit Sub
        End If

        If dtgVacunas.Rows(e.RowIndex).Cells("OtrasAdmisiones").Value.ToString.Trim.Length > 0 Then
            If dtgVacunas.Rows(e.RowIndex).Cells("OtrasAdmisiones").Value.ToString.Trim.IndexOf(((e.ColumnIndex \ 4) + 1).ToString) >= 0 Then
                Exit Sub
            End If
        End If


        Select Case e.ColumnIndex
            Case 2, 6, 10, 14, 18, 22
                If dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex).EditedFormattedValue = True Then
                    Select Case Val(dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex + 3).Value)
                        Case 2
                            dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex + 3).Value = 1
                        Case 3
                            dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex + 3).Value = 1
                        Case 6
                            dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex + 3).Value = 0
                    End Select
                    'dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex + 1).ErrorText = "Dato Obligatorio"
                    'dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex + 2).ErrorText = "Dato Obligatorio"
                    My.Computer.Keyboard.SendKeys("{TAB}")
                ElseIf dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex).EditedFormattedValue = False Then
                    dtTable = dtgVacunas.DataSource
                    If dtTable.Rows(e.RowIndex).Item("obs").ToString.Trim.Length > 0 Then
                        For intDosis = 1 To 6
                            If intDosis = 1 Then
                                intCol = 2
                            Else
                                intCol += 4
                            End If
                            If intCol <> e.ColumnIndex Then
                                If dtTable.Rows(e.RowIndex).Item("seleccion" & intDosis) = True Then
                                    If dtgVacunas.Rows(e.RowIndex).Cells(intCol + 3).Value = 3 Then
                                        dtgVacunas.Rows(e.RowIndex).Cells(intCol + 3).Value = 1
                                        blnConObs = False
                                        Exit For
                                    End If
                                End If
                            End If
                        Next
                        If blnConObs = True Then
                            dtgVacunas.Rows(e.RowIndex).Cells("obs").Value = DBNull.Value
                        End If
                    End If
                    dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex + 1).Value = DBNull.Value
                    dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex + 2).Value = DBNull.Value
                    Select Case Val(dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex + 3).Value)
                        Case 1, 3
                            dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex + 3).Value = 2
                        Case 0
                            dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex + 3).Value = 6
                    End Select
                    dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex + 1).ErrorText = ""
                    dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex + 2).ErrorText = ""
                End If

        End Select
    End Sub

    Private Sub dtgVacunas_CellEndEdit(ByVal sender As Object, ByVal e As System.Windows.Forms.DataGridViewCellEventArgs) Handles dtgVacunas.CellEndEdit
        '0=Adicion, 1=Modificacion, 2=Eliminación
        '3=Viene de Consulta
        '6=Viene Null de Consulta

        Dim intCol As Integer

        Select Case e.ColumnIndex
            Case 3, 7, 11, 15, 19, 23
                If Val(dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex + 2).Value) = 3 Then
                    dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex + 2).Value = 1
                End If
                If dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex).Value.ToString.Trim.Length > 0 Then
                    dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex).ErrorText = ""
                Else
                    'dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex).ErrorText = "Dato Obligatorio"
                End If
            Case 4, 8, 12, 16, 20, 24
                If ValidoMesAnno(ReemplazarString(dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex - 1).Value, DBNull.Value, ""), ReemplazarString(dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex).Value, DBNull.Value, "")) = 2 Then
                    MessageBox.Show("Mes y Año Son Invalidos para este Registro")
                    dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex).Value = DBNull.Value
                    dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex).ErrorText = "Dato Obligatorio"
                    Exit Sub
                End If
                If Val(dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex + 1).Value) = 3 Then
                    dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex + 1).Value = 1
                End If
                dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex).ErrorText = ""
            Case 27
                For intCol = 2 To 22 Step 4
                    If dtgVacunas.Rows(e.RowIndex).Cells(intCol).EditedFormattedValue = True Then
                        If Val(dtgVacunas.Rows(e.RowIndex).Cells(intCol + 3).Value) = 3 Then
                            dtgVacunas.Rows(e.RowIndex).Cells(intCol + 3).Value = 1
                        End If
                        Exit For
                    End If
                Next
                dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex).Value = dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex).Value.ToString.ToUpper
        End Select
    End Sub

    Private Sub cmdGuardarVacunas_Click(ByVal sender As Object, ByVal e As System.EventArgs) Handles cmdGuardarVacunas.Click
        Dim dtTablaVacunas As DataTable
        Dim dtTable As DataTable
        Dim dtRows() As DataRow
        Dim dtRow As DataRow
        Dim dtRowVacuna As DataRow
        Dim intDosis As Integer
        Dim strXMLVacunas As String
        Dim strVacunasObs As String


        If Me.ValidarCamposObligatoriosDatosVacunas = False Then
            MessageBox.Show("Falta Información en Campo Obligatorio", "Vacunas", MessageBoxButtons.OK, MessageBoxIcon.Error)
            Exit Sub
        End If

        dtTable = dtgVacunas.DataSource
        dtTablaVacunas = CrearDatosVacunasActualizar()
        strVacunasObs = ""
        For intDosis = 1 To 6
            dtRows = dtTable.Select("action" & intDosis.ToString & " <> 6 AND action" & intDosis.ToString & " <> 3")
            For Each dtRow In dtRows
                dtRowVacuna = dtTablaVacunas.NewRow()
                dtRowVacuna("vacuna") = dtRow("vacuna")
                dtRowVacuna("dosis") = intDosis
                dtRowVacuna("ano_dosis") = dtRow("anno" & intDosis.ToString)
                dtRowVacuna("mes_dosis") = dtRow("mes" & intDosis.ToString)
                dtRowVacuna("accion") = dtRow("action" & intDosis.ToString)
                If Val(dtRow("action" & intDosis.ToString)) <> 2 Then
                    If strVacunasObs.IndexOf(dtRow("vacuna").ToString.Trim & ",") < 0 Then
                        dtRowVacuna("obs") = dtRow("obs")
                        strVacunasObs &= dtRow("vacuna").ToString.Trim & ","
                    Else
                        dtRowVacuna("obs") = DBNull.Value
                    End If
                Else
                    dtRowVacuna("obs") = DBNull.Value
                End If
                dtTablaVacunas.Rows.Add(dtRowVacuna)
            Next
        Next


        If dtTablaVacunas.Rows.Count > 0 Then
            strXMLVacunas = GenerarXMLxProcedimiento(dtTablaVacunas)
        Else
            'MessageBox.Show("No Hay Modificaciones")
            Exit Sub
        End If

        ModificarVacunas(strXMLVacunas)


    End Sub

    Private Sub ModificarVacunas(ByVal strXMLVacunas As String)
        Dim objActualizarAP As New BLHistoriaBasica
        Dim Datos() As Object
        Dim lError As Long
        Dim intRow As Integer
        Dim intCol As Integer

        ReDim Datos(13)
        Datos(0) = 1
        Datos(1) = objDatosPaciente.TipoDocumento
        Datos(2) = objDatosPaciente.NumeroDocumento
        Datos(3) = ""
        Datos(4) = strXMLVacunas
        Datos(5) = objDatosGenerales.Prestador
        Datos(6) = objDatosGenerales.Sucursal
        Datos(7) = objDatosPaciente.TipoAdmision
        Datos(8) = objDatosPaciente.AnoAdmision
        Datos(9) = objDatosPaciente.NumeroAdmision
        Datos(10) = txtObservacionesAFar.Text.ToString
        Datos(11) = "N"
        Datos(12) = objDatosGenerales.Login
        Datos(13) = lError

        Try
            lError = objActualizarAP.ActualizarVacunas(objConexion, lError, Datos)
            If lError <> 0 Then
                MessageBox.Show("Error al Modificar Datos Vacunas")
            Else
                objDatosGenerales.HistoriaClinicaTieneModificaciones = True
                For intRow = 0 To dtgVacunas.Rows.Count - 1
                    For intCol = 1 To 6
                        If dtgVacunas.Rows(intRow).Cells("action" & intCol.ToString).Value <> 6 And dtgVacunas.Rows(intRow).Cells("action" & intCol.ToString).Value <> 3 Then
                            dtgVacunas.Rows(intRow).Cells("action" & intCol.ToString).Value = ActualizarFila(Val(dtgVacunas.Rows(intRow).Cells("action" & intCol.ToString).Value))
                        End If
                    Next
                Next
                dtgVacunas.Height = dtgVacunas.ColumnHeadersHeight + (dtgVacunas.Rows.GetRowsHeight(DataGridViewElementStates.None))
            End If
        Catch ex As Exception
            MessageBox.Show("Error al Modificar Datos Vacunas")
        End Try
    End Sub

    Private Function ActualizarFila(ByVal intValor As Integer) As Integer
        Select Case intValor
            Case 0, 1
                Return 3
            Case 2
                Return 6
        End Select

    End Function

    Private Function CrearDatosVacunasActualizar() As DataTable
        Dim dtTable As New DataTable

        With dtTable.Columns
            .Add("vacuna", System.Type.GetType("System.String"))
            .Add("dosis", System.Type.GetType("System.Int32"))
            .Add("ano_dosis", System.Type.GetType("System.Int32"))
            .Add("mes_dosis", System.Type.GetType("System.Int32"))
            .Add("accion", System.Type.GetType("System.Int32"))
            .Add("obs", System.Type.GetType("System.String"))
        End With

        Return dtTable
    End Function

