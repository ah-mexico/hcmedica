    Private Sub AdicionarNuevoAntecedenteFamiliar()
        Dim objActualizarAP As New BLHistoriaBasica
        Dim Datos() As Object
        Dim lError As Long
        Dim dtRows As DataRow()
        Dim dtRow As DataRow

        If dtgAntecedentesFamiliares.DataSource.Rows.Count > 0 Then
            dtRows = dtgAntecedentesFamiliares.DataSource.Select("cod_pre_sgs='" & objDatosGenerales.Prestador & "' AND cod_sucur='" & objDatosGenerales.Sucursal & _
                    "' AND tip_admision='" & objDatosPaciente.TipoAdmision & "' AND ano_adm=" & objDatosPaciente.AnoAdmision & " AND num_adm=" & objDatosPaciente.NumeroAdmision & _
                    " AND diagnost='" & txtCodDiagnosticoAF.Text.ToString & "' AND parentesco='" & txtCodParentescoAF.Text.ToString & "'")

            If dtRows.Length > 0 Then
                MessageBox.Show("El Registro Ya Existe")
                Exit Sub
            End If

            dtRows = dtgAntecedentesFamiliares.DataSource.Select("diagnost='" & txtCodDiagnosticoAF.Text.ToString & "'")

            If dtRows.Length > 0 Then
                If MessageBox.Show("Este Diagnóstico Ya Existe en los Antecedentes. Desea Registrarlo Nuevamente?", "Antecedentes Familiares", MessageBoxButtons.YesNo) = DialogResult.No Then
                    Exit Sub
                End If
            End If
        End If

        ReDim Datos(17)
        Datos(0) = 0
        Datos(1) = objDatosPaciente.TipoDocumento
        Datos(2) = objDatosPaciente.NumeroDocumento
        Datos(3) = objDatosPaciente.XMLDocumentosUnificados
        Datos(4) = objDatosGenerales.Prestador
        Datos(5) = objDatosGenerales.Sucursal
        Datos(6) = objDatosPaciente.TipoAdmision
        Datos(7) = objDatosPaciente.AnoAdmision
        Datos(8) = objDatosPaciente.NumeroAdmision
        Datos(9) = txtCodDiagnosticoAF.Text.ToString
        Datos(10) = txtCodParentescoAF.Text.ToString
        Datos(11) = ""
        Datos(12) = ""
        Datos(13) = cboDescDiagnosticoAF.OrigenDeDatos.Select("codigo='" & txtCodDiagnosticoAF.Text & "'")(0).Item("Clasificacion").ToString
        Datos(14) = txtObservacionesAF.Text.ToString
        Datos(15) = IIf(chkConfidencialAF.Checked = True, "S", "N")
        Datos(16) = objDatosGenerales.Login
        Datos(17) = lError

        Try
            lError = objActualizarAP.ActualizarAntecedenteFamiliar(objConexion, lError, Datos)
            If lError <> 0 Then
                MessageBox.Show("Error al Grabar Datos Antecedentes Familiares")
            Else
                objDatosGenerales.HistoriaClinicaTieneModificaciones = True
                dtRow = dtgAntecedentesFamiliares.DataSource.NewRow()
                With dtRow
                    .Item("cod_pre_sgs") = Datos(4)
                    .Item("cod_sucur") = Datos(5)
                    .Item("tip_doc") = Datos(1)
                    .Item("num_doc") = Datos(2)
                    .Item("tip_admision") = Datos(6)
                    .Item("ano_adm") = Datos(7)
                    .Item("num_adm") = Datos(8)
                    .Item("diagnost") = Datos(9)
                    .Item("descripcion") = cboDescDiagnosticoAF.Text.ToString
                    .Item("clasificacion") = Datos(13)
                    .Item("parentesco") = Datos(10)
                    .Item("dParentesco") = txtCodParentescoAF.DescripcionCodigo.ToString
                    .Item("obs") = Datos(14)
                    .Item("confidencial") = Datos(15)
                    .Item("medico") = objDatosGenerales.NombreMedico
                    .Item("fec_con") = FechaServidor()
                    .Item("sucursal") = "" 'NVL(objDatosGenerales.DescripcionSucursal)
                    .Item("EstadoUsu") = "N"
                End With
                dtgAntecedentesFamiliares.DataSource.Rows.Add(dtRow)
                dtgAntecedentesFamiliares.Height = dtgAntecedentesFamiliares.ColumnHeadersHeight + (dtgAntecedentesFamiliares.Rows.GetRowsHeight(DataGridViewElementStates.None))
                LimpiarPantallaAntecedentesFamiliares()

            End If
        Catch ex As Exception
            MessageBox.Show("Error al Grabar Datos Antecedentes Familiares")
        End Try

