                If MessageBox.Show("Esta Seguro de Eliminar El Registro de Examen Físico", "EXAMEN FISICO", MessageBoxButtons.YesNo, MessageBoxIcon.Question) = DialogResult.Yes Then
                    EliminarExamenFisico()
