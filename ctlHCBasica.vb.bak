    Private Sub ckbRefiereTodas_CheckedChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles ckbRefiereTodas.CheckedChanged
        Try


            Dim intRow As Integer
            Dim bRefiere As Boolean
            bRefiere = ckbRefiereTodas.Checked
            For intRow = 0 To dtgRevisionSistemasMC.Rows.Count - 1
                dtgRevisionSistemasMC.Rows(intRow).Cells("Refiere").Value = bRefiere
                If bRefiere Then
                    dtgRevisionSistemasMC.Rows(intRow).Cells("obsMC").Value = dtgRevisionSistemasMC.Rows(intRow).Cells("copiaobsMC").Value
                Else
                    dtgRevisionSistemasMC.Rows(intRow).Cells("copiaobsMC").Value = dtgRevisionSistemasMC.Rows(intRow).Cells("obsMC").Value
                    dtgRevisionSistemasMC.Rows(intRow).Cells("obsMC").Value = String.Empty
                End If
            Next
        Catch ex As Exception

        End Try
