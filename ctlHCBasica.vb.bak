    Private Sub dtgVacunas_CellBeginEdit(ByVal sender As Object, ByVal e As System.Windows.Forms.DataGridViewCellCancelEventArgs) Handles dtgVacunas.CellBeginEdit

        Select Case e.ColumnIndex
            Case 3, 7, 11, 15, 19, 23
                If dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex - 1).Value = False Then
                    e.Cancel = True
                Else
                    If dtgVacunas.Rows(e.RowIndex).Cells("OtrasAdmisiones").Value.ToString.Trim.Length > 0 Then
                        If dtgVacunas.Rows(e.RowIndex).Cells("OtrasAdmisiones").Value.ToString.Trim.IndexOf(((e.ColumnIndex \ 4) + 1).ToString) >= 0 Then
                            e.Cancel = True
                        End If
                    End If
                End If
            Case 4, 8, 12, 16, 20, 24
                If dtgVacunas.Rows(e.RowIndex).Cells(e.ColumnIndex - 2).Value = False Then
                    e.Cancel = True
                Else
                    If dtgVacunas.Rows(e.RowIndex).Cells("OtrasAdmisiones").Value.ToString.Trim.Length > 0 Then
                        If dtgVacunas.Rows(e.RowIndex).Cells("OtrasAdmisiones").Value.ToString.Trim.IndexOf(((e.ColumnIndex \ 4)).ToString) >= 0 Then
                            e.Cancel = True
                        End If
                    End If
                End If
            Case 27
                If dtgVacunas.Rows(e.RowIndex).Cells(2).Value = False And _
                   dtgVacunas.Rows(e.RowIndex).Cells(6).Value = False And _
                   dtgVacunas.Rows(e.RowIndex).Cells(10).Value = False And _
                   dtgVacunas.Rows(e.RowIndex).Cells(14).Value = False And _
                   dtgVacunas.Rows(e.RowIndex).Cells(18).Value = False And _
                   dtgVacunas.Rows(e.RowIndex).Cells(22).Value = False Then
                    e.Cancel = True
                End If
            Case 2, 6, 10, 14, 18, 22
                If dtgVacunas.Rows(e.RowIndex).Cells("OtrasAdmisiones").Value.ToString.Trim.Length > 0 Then
                    If dtgVacunas.Rows(e.RowIndex).Cells("OtrasAdmisiones").Value.ToString.Trim.IndexOf(((e.ColumnIndex \ 4) + 1).ToString) >= 0 Then
                        e.Cancel = True
                        Exit Sub
                    End If
                End If
        End Select
    End Sub

