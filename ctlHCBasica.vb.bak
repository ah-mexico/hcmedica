    Private Function ValidaTConsumo(ByVal strDescTiempoConsumo As String, ByVal iCantTiempoConsumoAT As Integer) As Double

        Dim dCantidad As Double

        Select Case strDescTiempoConsumo
            Case "AÑOS"
                Select Case objDatosPaciente.DescripcionUnidadMedidaEdad
                    Case "AÑOS"
                        dCantidad = iCantTiempoConsumoAT
                    Case "MESES"
                        dCantidad = iCantTiempoConsumoAT * 12
                    Case "DIAS"
                        dCantidad = iCantTiempoConsumoAT * 365
                End Select
            Case "MESES"
                Select Case objDatosPaciente.DescripcionUnidadMedidaEdad
                    Case "AÑOS"
                        dCantidad = iCantTiempoConsumoAT / 12
                    Case "MESES"
                        dCantidad = iCantTiempoConsumoAT
                    Case "DIAS"
                        dCantidad = iCantTiempoConsumoAT * 30
                End Select
            Case "SEMANAS"
                Select Case objDatosPaciente.DescripcionUnidadMedidaEdad
                    Case "AÑOS"
                        dCantidad = iCantTiempoConsumoAT / 52
                    Case "MESES"
                        dCantidad = iCantTiempoConsumoAT / 4
                    Case "DIAS"
                        dCantidad = iCantTiempoConsumoAT * 7
                End Select
            Case "DIAS"
                Select Case objDatosPaciente.DescripcionUnidadMedidaEdad
                    Case "AÑOS"
                        dCantidad = iCantTiempoConsumoAT / 365
                    Case "MESES"
                        dCantidad = iCantTiempoConsumoAT / 30
                    Case "DIAS"
                        dCantidad = iCantTiempoConsumoAT
                End Select
        End Select

        Return dCantidad

    End Function

    Private Sub txtDescTiempoConsumoAT_Validating(ByVal sender As Object, ByVal e As System.ComponentModel.CancelEventArgs) Handles txtDescTiempoConsumoAT.Validating
        If objDatosPaciente.Edad < ValidaTConsumo(txtDescTiempoConsumoAT.Text, Val(txtCantTiempoConsumoAT.Text)) Then
