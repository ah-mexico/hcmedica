    Private Sub CalcularValorGestacion(ByVal sender As Object, ByVal e As System.ComponentModel.CancelEventArgs) Handles txtNPartosAG.Validating, txtNCesareasAG.Validating, txtNAbortosAG.Validating, txtNEctopicosAG.Validating, txtNVivosAG.Validating, txtNMuertosAG.Validating
        Dim intValorGestacion As Integer


        Select Case sender.Name.ToString.ToLower
            Case "txtnpartosag", "txtncesareasag", "txtnabortosag", "txtnectopicosag"
                intValorGestacion = Val(txtNPartosAG.Text) + Val(txtNCesareasAG.Text) + Val(txtNAbortosAG.Text) + Val(txtNEctopicosAG.Text)
                If intValorGestacion > 30 Then
                    MessageBox.Show("Valor Excede Limite para Valor Gestacion (Máx. 30)", "Antecedentes Ginecológicos", MessageBoxButtons.OK, MessageBoxIcon.Information)
                    sender.ResetText()
                    sender.Focus()
                    Exit Sub
                End If
                txtNGestacionAG.Text = Val(txtNPartosAG.Text) + Val(txtNCesareasAG.Text) + Val(txtNAbortosAG.Text) + Val(txtNEctopicosAG.Text)
                If Val(txtNGestacionAG.Text) = 0 Then
                    txtNVivosAG.ReadOnly = True
                    txtNVivosAG.ResetText()
                    txtNMuertosAG.ReadOnly = True
                    txtNMuertosAG.ResetText()
                Else
                    txtNVivosAG.ReadOnly = False
                    txtNMuertosAG.ReadOnly = False
                End If
        End Select
