    Private Sub actualizarIncapacidad(ByVal strCodDiagnostico As String, ByVal strDescDiagnostico As String, _
                                    ByVal txtObs As String, ByVal codCategoria As String)

        Dim objIncapacidad As PlanIncapacidad

        ''Si es diagnostico principal y es una historia clinica de consulta externa 
        ''Se debe actualizar el objeto de la incapacidad
        If codCategoria = "P" And objDatosPaciente.TipoAdmision = Paciente.CONSULTAEXTERNA Then
            objIncapacidad = PlanIncapacidad.Instancia()
            With objIncapacidad
                .CodigoDiagnostico = strCodDiagnostico
                .Diagnostico = strDescDiagnostico
                .Observacion = txtObs
            End With
        End If
    End Sub


