    Private Sub cmdGuardarEID_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles cmdGuardarEID.Click
        If resPre1 = "" And resPre2 = "" And modPlanMaqn = "S" Then
            MsgBox("De respuesta a las preguntas de explicacion del plan y conciliacion de medicamentos.")
            rdPlaManPrg1Si.Focus()
            Return

        End If

        If ValidarCamposVaciosEID() = True Then
            If objHistoriaBasica.ImpresionDiagnostica.PermitirEliminarRegistro = True Then
                If MessageBox.Show("Esta Seguro de Eliminar El Registro de Egreso Impresión Diagnóstica", "IMPRESION DIAGNOSTICA", MessageBoxButtons.YesNo, MessageBoxIcon.Question) = DialogResult.Yes Then
                    EliminarEgresoImpresionDiagnostica()
                    txtCodCausaExternaID.Focus()
                End If
            End If
        Else
            If ValidarCamposObligatorios(pnlDatosEgresoImpresionDiagnostica) = False Then
                MessageBox.Show("Falta Información en Campo Obligatorio")
                Exit Sub
            End If
            ModificarEgresoImpresionDiagnostica()
        End If
        ModificarMotivoDeConsulta()
        ModificarRevisionSistemas()
        ModificarRiesgoNutricional()
        resPre1 = ""
        resPre2 = ""
        modPlanMaqn = ""
        grpGenPrg.Visible = False
    End Sub

