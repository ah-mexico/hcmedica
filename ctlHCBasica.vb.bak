    Private Sub AdicionarNuevoAntecedenteFarmacologico()
        Dim objActualizarAP As New BLHistoriaBasica
        Dim Datos() As Object
        Dim lError As Long
        Dim dtRows As DataRow()
        Dim dtRow As DataRow

        If dtgAntecedentesFarmacologicos.DataSource.Rows.Count > 0 Then
            dtRows = dtgAntecedentesFarmacologicos.DataSource.Select("cod_pre_sgs='" & objDatosGenerales.Prestador & "' AND cod_sucur='" & objDatosGenerales.Sucursal & _
                          "' AND tip_admision='" & objDatosPaciente.TipoAdmision & "' AND ano_adm=" & objDatosPaciente.AnoAdmision & " AND num_adm=" & objDatosPaciente.NumeroAdmision & _
                          " AND producto='" & txtCodFarmacoAFar.Text.ToString & "'")

            If dtRows.Length > 0 Then
                MessageBox.Show("Registro Ya Existe")
                Exit Sub
            End If

            dtRows = dtgAntecedentesFarmacologicos.DataSource.Select("producto='" & txtCodFarmacoAFar.Text.ToString & "'")
            If dtRows.Length > 0 Then
                If MessageBox.Show("Este Producto Ya Existe en los Antecedentes. Desea Registrarlo Nuevamente?", "Antecedentes Farmacológicos", MessageBoxButtons.YesNo) = DialogResult.No Then
                    Exit Sub
                End If
            End If
        End If

        ReDim Datos(13)
        Datos(0) = 0
        Datos(1) = objDatosPaciente.TipoDocumento
        Datos(2) = objDatosPaciente.NumeroDocumento
        Datos(3) = objDatosPaciente.XMLDocumentosUnificados
        Datos(4) = objDatosGenerales.Prestador
        Datos(5) = objDatosGenerales.Sucursal
        Datos(6) = objDatosPaciente.TipoAdmision
        Datos(7) = objDatosPaciente.AnoAdmision
        Datos(8) = objDatosPaciente.NumeroAdmision
        Datos(9) = txtCodFarmacoAFar.Text.ToString
        Datos(10) = txtObservacionesAFar.Text.ToString
        Datos(11) = IIf(chkConfidencialAFar.Checked = True, "S", "N")
        Datos(12) = objDatosGenerales.Login
        Datos(13) = lError

        Try
            lError = objActualizarAP.ActualizarAntecedenteFarmacologico(objConexion, lError, Datos)
            If lError <> 0 Then
                MessageBox.Show("Error al Grabar Datos Antecedentes Farmacologicos")
            Else
                objDatosGenerales.HistoriaClinicaTieneModificaciones = True
                dtRow = dtgAntecedentesFarmacologicos.DataSource.NewRow()
                With dtRow
                    .Item("cod_pre_sgs") = Datos(4)
                    .Item("cod_sucur") = Datos(5)
                    .Item("tip_doc") = Datos(1)
                    .Item("num_doc") = Datos(2)
                    .Item("tip_admision") = Datos(6)
                    .Item("ano_adm") = Datos(7)
                    .Item("num_adm") = Datos(8)
                    .Item("producto") = Datos(9)
                    .Item("Descripcion") = cboMedicamentosAF.Text
                    .Item("obs") = Datos(10)
                    .Item("confidencial") = Datos(11)
                    .Item("medico") = objDatosGenerales.NombreMedico
                    .Item("EstadoUsu") = "N"
                End With
                dtgAntecedentesFarmacologicos.DataSource.Rows.Add(dtRow)
                dtgAntecedentesFarmacologicos.Height = dtgAntecedentesFarmacologicos.ColumnHeadersHeight + (dtgAntecedentesFarmacologicos.Rows.GetRowsHeight(DataGridViewElementStates.None))
                LimpiarPantallaAntecedentesFarmacologicos()
            End If
        Catch ex As Exception
            MessageBox.Show("Error al Grabar Datos Antecedentes Farmacologicos")
        End Try

    End Sub

