    Private Sub ActualizarInformacionControles()
        Dim intRow As Integer

        txtInfoSuministradaPor.Tag = txtInfoSuministradaPor.Text
        txtMotivoConsulta.Tag = txtMotivoConsulta.Text
        txtEnfermedadActual.Tag = txtEnfermedadActual.Text
        For intRow = 0 To dtgRevisionSistemasMC.Rows.Count - 1
            'dtgRevisionSistemasMC.Rows(intRow).Cells("obsMC").Value = dtgRevisionSistemasMC.Rows(intRow).Cells("obsMC").Value.ToString.ToUpper
            'dtgRevisionSistemasMC.Rows(intRow).Cells("copiaobsMC").Value = dtgRevisionSistemasMC.Rows(intRow).Cells("obsMC").Value
