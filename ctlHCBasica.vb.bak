    Private Function ValidarCamposVaciosEF() As Boolean
        Dim blnRespuesta As Boolean
        Dim dtTable As DataTable
        Dim intReg As Integer


        blnRespuesta = txtSistoleEF.Text.Trim.Length = 0 And _
                       txtDiastoleEF.Text.Trim.Length = 0 And _
                       txtFrecuenciaRespiratoriaEF.Text.Trim.Length = 0 And _
                       txtFrecuenciaCardiacaEF.Text.Trim.Length = 0 And _
                       txtTemperaturaEF.Text.Trim.Length = 0 And _
                       txtPesoEF.Text.Trim.Length = 0 And _
                       txtTallaEF.Text.Trim.Length = 0 And _
                       txtPerimetroCefalicoEF.Text.Trim.Length = 0 And _
                       txtPerimetroAbdominalEF.Text.Trim.Length = 0 And _
                       txtGasglowEF.Text.Trim.Length = 0 And _
                       txtCodEstadoConcienciaEF.Text.Trim.Length = 0 And _
                       txtEstadoGeneralEF.Text.Trim.Length = 0 And _
                       optEmbriaguezNO.Checked = False And _
                       optEmbriaguezSI.Checked = False And _
                       chkConfidencialEF.Checked = False

        If blnRespuesta = True Then
            dtTable = dtgHallazgosEF.DataSource
            For intReg = 1 To dtTable.Rows.Count - 1
                If dtTable.Rows(intReg).Item("obs").ToString.Trim.Length > 0 Then
                    Return False
                End If
            Next
            Return True
        Else

            Return False

        End If

    End Function

    Private Sub pnlExamenFisico_Validating(ByVal sender As Object, ByVal e As System.ComponentModel.CancelEventArgs) Handles pnlExamenFisico.Validating

        Dim blnModificaciones As Boolean
        Dim intRow As Integer

        If txtSistoleEF.Text = txtSistoleEF.Tag And txtDiastoleEF.Text = txtDiastoleEF.Tag And _
            txtFrecuenciaRespiratoriaEF.Text = txtFrecuenciaRespiratoriaEF.Tag And txtFrecuenciaCardiacaEF.Text = txtFrecuenciaCardiacaEF.Tag And _
            txtTemperaturaEF.Text = txtTemperaturaEF.Tag And txtPesoEF.Text = txtPesoEF.Tag And _
            txtTallaEF.Text = txtTallaEF.Tag And txtPerimetroCefalicoEF.Text = txtPerimetroCefalicoEF.Tag And _
            txtPerimetroAbdominalEF.Text = txtPerimetroAbdominalEF.Tag And txtGasglowEF.Text = txtGasglowEF.Tag And _
            optEmbriaguezNO.Checked = optEmbriaguezSI.Tag And optEmbriaguezSI.Checked = optEmbriaguezSI.Tag And _
            txtCodEstadoConcienciaEF.Text = txtCodEstadoConcienciaEF.Tag And txtDescEstadoConcienciaEF.Text = txtDescEstadoConcienciaEF.Tag And _
            txtEstadoGeneralEF.Text = txtEstadoGeneralEF.Tag And chkConfidencialEF.Checked = chkConfidencialEF.Tag Then
            For intRow = 0 To dtgHallazgosEF.Rows.Count - 1
                If NVL(dtgHallazgosEF.Rows(intRow).Cells("obsEF").Value) <> NVL(dtgHallazgosEF.Rows(intRow).Cells("copiaobsEF").Value) Then
                    blnModificaciones = True
                    Exit For
                End If
            Next
        Else
            blnModificaciones = True
        End If

        If blnModificaciones = False Then
            Exit Sub
        End If

        Me.cmdGuardarEF_Click(sender, e)

    End Sub

