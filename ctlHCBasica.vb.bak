    Private Sub bgwCargarInfoInicial2_DoWork(ByVal sender As Object, ByVal e As System.ComponentModel.DoWorkEventArgs) Handles bgwCargarInfoInicial2.DoWork
        Try
            TraerDatosAntecedentesAlergicos()
            TraerDatosAntecedentesToxicos()
            TraerDatosAntecedentesFamiliares()
            TraerDatosVacunas()
            TraerDatosExamenFisico()
