    Private Sub AdicionarNuevoAntecedenteAlergico()
        Dim objActualizarAP As New BLHistoriaBasica
        Dim Datos() As Object
        Dim lError As Long
        Dim dtRows As DataRow()
        Dim dtRow As DataRow

        If dtgAntecedentesAlergicos.DataSource.Rows.Count > 0 Then
            dtRows = dtgAntecedentesAlergicos.DataSource.Select("cod_pre_sgs='" & objDatosGenerales.Prestador & "' AND cod_sucur='" & objDatosGenerales.Sucursal & _
                    "' AND tip_admision='" & objDatosPaciente.TipoAdmision & "' AND ano_adm=" & objDatosPaciente.AnoAdmision & " AND num_adm=" & objDatosPaciente.NumeroAdmision & _
                    " AND alergico='0' AND clase='" & txtCodClaseAA.Text.ToString & "'")

            If dtRows.Length > 0 Then
                MessageBox.Show("El Registro Ya Existe")
                Exit Sub
            End If

            dtRows = dtgAntecedentesAlergicos.DataSource.Select("alergico='0' AND clase='" & txtCodClaseAA.Text.ToString & "'")
            If dtRows.Length > 0 Then
                If MessageBox.Show("Esta Clase de Alérgico Ya Existe en los Antecedentes. Desea Registrarlo Nuevamente?", "Antecedentes Alérgicos", MessageBoxButtons.YesNo) = DialogResult.No Then
                    Exit Sub
                End If
            End If
        End If

        ReDim Datos(14)
        Datos(0) = objDatosGenerales.Prestador
        Datos(1) = objDatosGenerales.Sucursal
        Datos(2) = objDatosPaciente.TipoDocumento
        Datos(3) = objDatosPaciente.NumeroDocumento
        Datos(4) = objDatosPaciente.TipoAdmision
        Datos(5) = objDatosPaciente.AnoAdmision
        Datos(6) = objDatosPaciente.NumeroAdmision
        Datos(7) = ReemplazarString(Val(txtCodClaseAA.Text.ToString), 0, DBNull.Value)
        Datos(8) = objDatosGenerales.Login
        Datos(9) = txtAgenteCausal.Text.ToString
        Datos(10) = ReemplazarString(Val(txtCodMesAA.Text.ToString), 0, DBNull.Value)
        Datos(11) = ReemplazarString(Val(txtAnnoAA.Text.ToString), 0, DBNull.Value)
